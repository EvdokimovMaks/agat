)
With Index
(
 TTLim_for_Rep_Kol01  =  cTTLimKatSopr_Kol(Unique),
 TTLim_for_Rep_Kol02  =  BaseDoc_dDoc + BaseDoc_NoDoc + cTTLim_Kol + cPodrFrom + cPodrTo + dOpr + VidSopr + cMC + NSopr + cKatSopr,    // BaseDoc_dDoc + BaseDoc_NoDoc + cMC + cPodrFrom + cPodrTo  здесь поставлены для сортировки, для идентификации достаточно cTTLim_Kol
 TTLim_for_Rep_Kol03  =  cTTLim_Kol + Find_LimSopr(Desc) + dOpr(Desc) + NSopr(Desc)
);

