 KolDel501_Old : double,

 KolAdd501_Old : double,
 SumAdd501_Old : double,

 Kol503_Old    : double,
 Sum503_Old    : double
)
With Index
(
 TTLimBaseDoc_Kol001  =  Nrec(Unique, Surrogate),
 TTLimBaseDoc_Kol002  =  cBaseDoc + cMC + cParty,
