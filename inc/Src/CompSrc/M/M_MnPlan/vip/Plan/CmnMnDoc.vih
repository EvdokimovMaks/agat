// Структура для хранения состояния документа
#ifndef __Def_TMnDocState__
#define __Def_TMnDocState__
#doc
 Структура для хранения состояния документа</brief> <br>
 Поля: <br>
   wTypeDoc  - Тип документа <br>
   cMnDoc    - Cсылка на документ <br>
   cMnTbl    - Cсылка на таблицу документа <br>
   cMnStr    - Cсылка на строку таблицы документа <br>
   cMnCln    - Cсылка на столбец таблицы документа <br>
   cMnCell   - Cсылка на ячейку документа <br>
   cWayAccou - MnPlan: ссылка на шаблон документа <br>
   cMnPlan   - MnPlan: ссылка на документ <br>
   cAlgView  - MnPlan: ссылка на алгоритм представления <br>
   cSpMnPlan - MnPlan: ссылка на строку спецификации документа <br>
   cSpMnPl   - MnPlan: ссылка на позицию документа <br>
#end
Type TMnDocState = record
  wTypeDoc: word;  // Тип документа
  cMnDoc: comp;    // Cсылка на документ
  cMnTbl: comp;    // Cсылка на таблицу документа
  cMnStr: comp;    // Cсылка на строку таблицы документа
  cMnCln: comp;    // Cсылка на столбец таблицы документа
  cMnCell: comp;   // Cсылка на ячейку документа
  //
  cWayAccou: comp; // MnPlan: ссылка на шаблон документа
  cMnPlan: comp;   // MnPlan: ссылка на документ
  cAlgView: comp;  // MnPlan: ссылка на алгоритм представления
  cSpMnPlan: comp; // MnPlan: ссылка на строку спецификации документа
  cSpMnPl: comp;   // MnPlan: ссылка на позицию документа
End; // Type TMnDocState = record
#endif

