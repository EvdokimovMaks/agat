#ifndef __OINASTSRV_VIH
#define __OINASTSRV_VIH

#ifdef ComponentVersion
#component "M_TPP"
#end

#doc
объектный интерфейс для работы со значениями параметров</brief>
#end

ObjInterface oiNastSrv;

#doc
установка владельца значений параметров шаблона</brief>:<br>
<I>wcDocNastr</I> - ключ шаблона
#end 
  function SetOwner(wcDocNastr: comp): integer;

#doc
установка значения параметра шаблона</brief>:<br>
<I>wZaklCode</I> - код группы параметров
<I>wCode</I>     - код назначения параметра
<I>wIndx</I>     - индекс параметра в группе
<I>wType</I>     - тип значения параметра
<I>sValue</I>    - новое значение
#end 
  function SetValue(wZaklCode: integer; wCode:integer; wIndx: integer; wType:integer; sValue: string): integer;

#doc
получение значения параметра шаблона</brief>:<br>
<I>wZaklCode</I> - код группы параметров
<I>wCode</I>     - код назначения параметра
<I>wIndx</I>     - индекс параметра в группе
<I>wType</I>     - тип значения параметра
#end 
  function GetValue(wZaklCode: integer; wCode:integer; wIndx: integer; wType:integer): string;

#doc
вставка параметра в шаблон</brief>:<br>
<I>wZaklCode</I> - код группы параметров
<I>wCode</I>     - код назначения параметра
#end
  function xInsert(wZaklCode: integer; wCode:integer): integer;

#doc
удаление группы значений параметров</brief>:<br>
<I>wZaklCode</I> - код группы параметров
#end
  function xSPDelete(wZaklCode: integer): integer;

#doc
удаление значений параметров шаблона</brief>:<br>
<I>cNastr</I> - ключ шаблона
#end
  function xDelete(cNastr: comp): integer;
end; // oiNastSrv

#endif  // __OINASTSRV_VIH