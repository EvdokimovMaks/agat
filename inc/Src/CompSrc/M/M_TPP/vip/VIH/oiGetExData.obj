#ifndef __OIGETEXDATA_VIH
#define __OIGETEXDATA_VIH

#include oiGPSrv01.obj // установка/получение параметров
#include oiGPSrv03.obj // работа с журналом событий
#include oiListSrv.obj // работа с объектом oAlgInDocList

#ifdef ComponentVersion
#component "M_TPP"
#end

type
// исходные данные
  rExDataParam = record
                  cInfo$1, // параметры comp
                  cInfo$2,
                  cInfo$3,
                  cInfo$4
                         : comp;
                  wInfo$1, // параметры word
                  wInfo$2,
                  wInfo$3
                         : word;
                 dtInfo$1  // параметры date
                         : date
                  dInfo$1  // параметры double
                         : double;
                 end;
// результат
  rExDataResult = record
                    wResult$1, // код возврата
                    wResult$2, // результаты word
                    wResult$3
                             : word;
                    cResult$1, // результаты comp
                    cResult$2
                             : comp;
                    dResult$1, // результаты double
                    dResult$2,
                    dResult$3,
                    dResult$4,
                    dResult$5,
                    dResult$6,
                    dResult$7,
                    dResult$8
                             : double;
                  end;

//******************************************************************************
#doc
Объектный интерфейс получения данных</brief>
#end
ObjInterface oiGetExData;
#doc
получить данные</brief>
#end
  function GetExData(pDataParam: rExDataParam): rExDataResult;

end; // oiGetExData

//******************************************************************************
#doc
Объектный интерфейс установки фиксированных исходных данных</brief>
#end
ObjInterface oiSetExParam;
#doc
установить исходные параметры</brief>
#end
  procedure SetExParam(pDataParam: rExDataParam);

end; // oiSetExParam

#endif  // __OIGETEXDATA_VIH