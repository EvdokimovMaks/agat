Table struct tpIerH
(
  nRec    : comp,
  wPodr   : word,
  cPodr   : comp,     //แแ๋ซช ญ ฏฎคเงคฅซฅญจฅ
  cPodrUp : comp,     //แแ๋ซช ญ ข๋่ฅแโ. ฏฎคเงคฅซฅญจฅ
  cTpIerH : comp,     //แแ๋ซช ญ ข๋่ฅแโ. TpIerH
  wNum    : word,     //ฏฎเ๏คชฎข๋ฉ ญฎฌฅเ
  Sum     : double
)
With Index
(
 tpIerH01 = nRec (Unique, Surrogate, Journal),
 tpIerH02 = cPodr + wPodr,
 tpIerH03 = cTpIerH,
 tpIerH04 = wNum,
 tpIerH05 = cPodrUp,
 tpIerH06 = wPodr + cPodr
);

Table struct tpSpIerH
(
  nRec    : comp,
  cChild  : comp,   //เฅกฅญฎช
  cParent : comp    //เฎคจโฅซ์
)
With Index
(
 tpSpIerH01 = nRec (Unique, Surrogate, Journal),
 tpSpIerH02 = cChild,
 tpSpIerH03 = cParent
);