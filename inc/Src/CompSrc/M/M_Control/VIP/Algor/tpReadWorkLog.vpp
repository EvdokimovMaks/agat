table struct tpELItem // элемент журнала
(
  nRec  : comp,
  wEObj : word,
  wEType: word,
  wECode: word,
  LCount: longint
)
with index
(
  tpELItem01 = nRec (Unique, Surrogate),
  tpELItem02 = wEObj + wEType + wECode
)
;

table struct tpELILine // текстовое описание элемента журнала
(
  cELItem: comp,
  wNom   : word,
  sWhat  : string[15],
  sInfo  : string
)
with index
(
  tpELILine01 = cELItem + wNom
)
;
