#ifdef ComponentVersion
#Component ""
#end

#define TabMem_SomePrice

Table Struct tpPriceRes
  (
   wPodr      : Word,       // поставщик
   cPodr      : comp,       //
   wRes       : Word,       // ресурс
   cRes       : comp,       //
   cParty     : comp,       // партия
   cValut     : comp,       // валюта (если не заказано, то в хранимой)
   Cena       : Double,     // цена за ЕИ (тип ЕИ заказывается)
   cPeriod    : comp,       //                               период действия цены
   DatNac     : Date,       // дата начала действия цены или начало периода
   dPerEnd    : Date        //                               конец периода
  )
with Index
  (
  tpPriceRes01 = wRes + cRes + cParty + wPodr + cPodr + DatNac,
  tpPriceRes02 = wPodr + cPodr + wRes + cRes + cParty + DatNac,
  tpPriceRes03 = wPodr + cPodr + wRes + cRes + DatNac,
  tpPriceRes04 = wRes + cRes + DatNac,
  tpPriceRes05 = wRes + cRes + cParty + DatNac
  );
