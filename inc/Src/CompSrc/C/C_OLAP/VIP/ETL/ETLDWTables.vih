#ifndef __ETLDWTables__
#define __ETLDWTables__

// список интерфейсов выгрузки
table struct tDWExporters
(
    NRec       :comp          "Идентификатор"
  , Identifier : string[150]  "Программное имя интерфейса"
)
with index
(
  tDWExporters01 = NRec(AutoInc, Unique),
  tDWExporters02 = Identifier
);

// таблица полей и параметров интерфейса выгрузки
table struct tDWExporterSchTbl
(
    NRec       : comp         "Идентификатор"
  , Identifier : string[150]  "Строковый идентификатор поля"
  , Name       : string[150]  "Наименование поля"
  , _Type      : longint      "Тип поля"
)
with index
(
  tDWExporterSchTbl01 = NRec(AutoInc, Unique),
  tDWExporterSchTbl02 = Identifier,
  tDWExporterSchTbl03 = Name + Identifier,
  tDWExporterSchTbl04 = _Type + Identifier
);

// таблица параметров и их значений для интерфейса выгрузки
table struct tDWExporterParams
(
    NRec           : comp         "Идентификатор"
  , Identifier     : string[150]  "Строковый идентификатор параметра"
  , UnitIdentifier : string[150]  "Название интерфейсы выгрузки"
  , Value          : string[250]  "Значение параметра"
)
with index
(
  tDWExporterParams01 = NRec(AutoInc, Unique),
  tDWExporterParams02 = Identifier,
  tDWExporterParams03 = UnitIdentifier + Identifier
);

#endif