Private ObjInterface ITRBTablUseRepTypeView(ITRB_BaseAbstractTblInc);  //#region ObjInterface

  //----------------------------------------------------------------------------
  //#region Property
  Property NRec      : comp       read write;  // Номер записи
  Property wGroup    : word       read write;  // Группа отчетов
  Property wRepID    : word       read write;  // Идентификатор отчета
  Property wRepVer   : word       read write;  // Версия отчета
  Property wTPerMask : word       read write;  // Типы периодов формирования
  Property cTaxOrg   : comp       read write;  // Ссылка на налогоплательщика

  Property sTPerName : string     read      ;  // Наименование типов периода
  //#endregion Property
  //----------------------------------------------------------------------------

  //----------------------------------------------------------------------------
  //#region Index
  Index TRBTablUseRepType01 = NRec(Unique, Primary);
  Index TRBTablUseRepType02 = cTaxOrg, wRepID;
  Index TRBTablUseRepType03 = cTaxOrg, wGroup;
  Index TRBTablUseRepType04 = wGroup;
  Index TRBTablUseRepType05 = wRepID;
  //#endregion Index
  //----------------------------------------------------------------------------

End; //#endregion ObjInterface
