#ifndef __gzrCompAlgorithm_Tbl__
#define __gzrCompAlgorithm_Tbl__

//------------------------------------------------------------------------------
// Таблица позиций комплексного алгоритма
Table struct TblComplexAlg
(
  NRec   : comp     // Номер записи
, Npp    : word     // Порядковый номер
, cAlg   : comp     // ссылка на алгоритм
, sKey   : string   // ключ алгоритма
, lCount : LongInt  // количество строк в алгоритме
)
With Index
(
  TblComplexAlg01  =  NRec(Unique, AutoInc)
, TblComplexAlg02  =  Npp
, TblComplexAlg03  =  cAlg
);

#endif//__gzrCompAlgorithm_Tbl__
