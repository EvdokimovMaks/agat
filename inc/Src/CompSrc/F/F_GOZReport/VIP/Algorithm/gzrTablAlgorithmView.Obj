#ifndef __gzrTablAlgorithmView_Obj__
#define __gzrTablAlgorithmView_Obj__

#Component "F_GOZReport"

//------------------------------------------------------------------------------
#include gzr_BaseAbstractTblInc.Obj  // Абстрактный базовый интерфейс : "Object Related Table"
//------------------------------------------------------------------------------

//==============================================================================
#doc
Табличный объект чтения : "Алгоритмы расчета"<br>
<b>Внимание!</b> Интерфейс предназначен только для внутреннего использования
#end
Private ObjInterface IgzrTablAlgorithmView(Igzr_BaseAbstractTblInc);  //#region ObjInterface

  //----------------------------------------------------------------------------
  //#region Property
  Property NRec      : comp       read write;  // Номер записи
  Property cNode     : comp       read write;  // Ссылка на вышестоящий
  Property wRepID    : word       read write;  // Идентификатор отчета
  Property wRepVer   : word       read write;  // Версия отчета
  Property sOwnKey   : s100       read write;  // Ключ собственного алгоритма
  Property sAddKey   : string     read write;  // Ключ дополнительного алгоритма
  Property Name      : TName      read write;  // Наименование
  Property Note      : string     read write;  // Описание
  Property IsAlg     : Boolean    read write;  // Признак алгоритма
  Property IsGroup   : Boolean    read write;  // Признак "группа алгоритмов"
  Property IsSysRec  : Boolean    read write;  // Признак "системная запись"
  Property IsAddAlg  : Boolean    read write;  // Признак "дополнительный алгоритм"
  //#endregion Property
  //----------------------------------------------------------------------------

  //----------------------------------------------------------------------------
  //#region Index
  Index gzrTablAlgorithm01 = NRec(Unique, Primary);
  Index gzrTablAlgorithm02 = cNode, Name;
  Index gzrTablAlgorithm03 = Name;
  Index gzrTablAlgorithm04 = sOwnKey;
  Index gzrTablAlgorithm05 = wRepID, wRepVer;
  //#endregion Index
  //----------------------------------------------------------------------------

  //============================================================================
  //#region BUFFER
  //----------------------------------------------------------------------------
  #doc
     Получить буфер TaxAlg с позицией
  #end
  function GetTaxAlgBuff : type$p_TaxAlg;
  //----------------------------------------------------------------------------
  #doc
     Установить буфер TaxAlg с позицией
  #end
  function SetTaxAlgBuff(_Value : type$p_TaxAlg) : boolean;
  //#endregion BUFFER
  //****************************************************************************

End; //#endregion ObjInterface
//******************************************************************************


#endif//__gzrTablAlgorithmView_Obj__
