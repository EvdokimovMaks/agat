#ifndef __gzrTablRepHeadView_Obj__
#define __gzrTablRepHeadView_Obj__

#Component "F_GOZReport"

//------------------------------------------------------------------------------
#include gzr_BaseAbstractTblInc.Obj  // Абстрактный базовый интерфейс : "Object Related Table"
//------------------------------------------------------------------------------

//==============================================================================
#doc
Табличный объект чтения : "Заголовок отчета"<br>
<b>Внимание!</b> Интерфейс предназначен только для внутреннего использования
#end
Private ObjInterface IgzrTablRepHeadView(Igzr_BaseAbstractTblInc);  //#region ObjInterface

  //----------------------------------------------------------------------------
  //#region Property
  Property NRec      : comp       read write;  // Номер записи
  Property DesGr     : TDesGr     read write;  // Код группы пользователей
  Property Descr     : TDescr     read write;  // Дескриптор пользователя
  Property wStatus   : word       read write;  // Статус отчета
  Property cNote     : comp       read write;  // Ссылка на пояснение к статусу
  Property Name      : TName      read write;  // Наименование
  Property wRepID    : word       read write;  // Идентификатор отчета
  Property wRepVer   : word       read write;  // Версия отчета
  Property cGOZOrg   : comp       read write;  // Ссылка на налогоплательщика
  Property wTPer     : word       read write;  // Тип периода
  Property cPeriod   : comp       read write;  // Ссылка на период
  Property dForm     : date       read write;  // Дата формирования
  Property dBeg      : date       read write;  // Начало периода
  Property dEnd      : date       read write;  // Конец периода
  Property cAlg      : comp       read write;  // Ссылка на алгоритм
  Property sAlgKey   : s100       read write;  // Ключ алгоритма
  Property sComment  : string     read write;  // Комментарий
  //#endregion Property
  //----------------------------------------------------------------------------

  //----------------------------------------------------------------------------
  //#region Index
  Index gzrTablRepHead01 = NRec(Unique, Primary);
  Index gzrTablRepHead02 = cGOZOrg, wRepID, wRepVer, cPeriod;
  Index gzrTablRepHead03 = cGOZOrg, cPeriod;
  //#endregion Index
  //----------------------------------------------------------------------------

  //----------------------------------------------------------------------------
  //#region Methods
  //----------------------------------------------------------------------------
  #doc
     Получить буфер GOZDat с позицией
  #end
  function GetGOZDatBuff : type$p_TaxDat;
  //----------------------------------------------------------------------------
  #doc
     Установить буфер GOZDat с позицией
  #end
  function SetGOZDatBuff(_Value : type$p_TaxDat) : boolean;

  //----------------------------------------------------------------------------
  #doc
     Разрешено ли показывать текущую запись
  #end
  function CanView : Boolean;
  //#endregion Methods
  //----------------------------------------------------------------------------

End; //#endregion ObjInterface
//******************************************************************************


#endif//__gzrTablRepHeadView_Obj__
