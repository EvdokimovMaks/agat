#ifndef __gzrTablSysRepTypeView_Obj__
#define __gzrTablSysRepTypeView_Obj__

#Component "F_GOZReport"

//------------------------------------------------------------------------------
#include gzr_BaseAbstractTblInc.Obj  // Абстрактный базовый интерфейс : "Object Related Table"
//------------------------------------------------------------------------------

//==============================================================================
#doc
Табличный объект чтения : "Системные типы отчетов"<br>
<b>Внимание!</b> Интерфейс предназначен только для внутреннего использования
#end
Private ObjInterface IgzrTablSysRepTypeView(Igzr_BaseAbstractTblInc);  //#region ObjInterface

  //----------------------------------------------------------------------------
  //#region Property
  Property NRec      : comp       read write;  // Номер записи
  Property wGroup    : word       read write;  // Группа отчетов
  Property wRepID    : word       read write;  // Идентификатор отчета
  Property wRepVer   : word       read write;  // Версия отчета
  Property dBeg      : date       read write;  // Дата начала действия
  Property dEnd      : date       read write;  // Дата окончания действия
  Property wTPerMask : word       read write;  // Типы периодов формирования
  Property Name      : TName      read write;  // Наименование
  Property Note      : string     read write;  // Описание
  Property sIFCAbbr  : string     read write;  // Аббревиатура интерфейсов отчета
  Property sAlgKey   : string     read write;  // Ключ системного алгоритма

  Property sTPerName : string     read      ;  // Наименование типов периода
  Property sIFCEditor: string     read      ;  // Наименование интерфейса редактирования
  Property sIFCSysAlg: string     read      ;  // Наименование интерфейса генерации кода "Системного алгоритма"
  Property sIFCEtStru: string     read      ;  // Наименование интерфейса генерации кода "Эталонной структуры"
  //#endregion Property
  //----------------------------------------------------------------------------

  //----------------------------------------------------------------------------
  //#region Index
  Index gzrTablSysRepType01 = NRec(Unique, Primary);
  Index gzrTablSysRepType02 = wGroup, Name;
  Index gzrTablSysRepType03 = wRepID, wRepVer;
  Index gzrTablSysRepType04 = wRepVer;
  //#endregion Index
  //----------------------------------------------------------------------------

End; //#endregion ObjInterface
//******************************************************************************


#endif//__gzrTablSysRepTypeView_Obj__
