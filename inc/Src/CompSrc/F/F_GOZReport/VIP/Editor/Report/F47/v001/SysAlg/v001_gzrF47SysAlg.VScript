          } // если удалось установить значение показателя
        }
        //#endregion 2. Распределение ресурсов контракта (ГруппаРаспределениеРесурсовКонтракта)
        //----------------------------------------------------------------------------------------------------

        //#endregion расчет полей Контракта
        //----------------------------------------------------------------------------------------------------

      } // побежали по Контрактам
      if (not isAllOk)
      { Break; }

      //------------------------------------------------------------------------------------------------------
      // начальная дата периода расчета
      if (not pCurRepEditor.SetDateBeg(dRepDateBeg) )
      {
        pRLogRepEditor.AddErrMessage(sMainAddr, pCurRepEditor.GetStLastError);
      }
      //------------------------------------------------------------------------------------------------------
      // конечная дата периода расчета
      if (not pCurRepEditor.SetDateEnd(dRepDateEnd) )
      {
        pRLogRepEditor.AddErrMessage(sMainAddr, pCurRepEditor.GetStLastError);
      }
      //------------------------------------------------------------------------------------------------------
    }
    while False;
    if (not isAllOk)
    { Break; }
    //#endregion Контракты
    //********************************************************************************************************

  }
  while False;
  Result := isAllOk;
@end. // On_Recalc
//#endregion ДЕЙСТВИЯ ВЫПОЛНЯЕМЫЕ ПРИ РАСЧЕТЕ
//************************************************************************************************************
