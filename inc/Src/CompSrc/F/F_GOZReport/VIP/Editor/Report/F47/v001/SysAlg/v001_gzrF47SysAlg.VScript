  //#region СЕРВИС
  //----------------------------------------------------------------------------------------------------------
  // Получить версию программы сформировавшей отчет
  function myGetVersProg : string;
  {
    var V1 : word;  V1 := 0;
    var V2 : word;  V2 := 0;
    var V3 : word;  V3 := 0;
    var V4 : word;  V4 := 0;
    VersionGet(False, 'F_PAT', V1, V2, V3, V4);
    Result := FormatStr_2('Галактика ERP %D.%D', V1, V2);
  } // function myGetVersProg
  //#endregion СЕРВИС
  //**********************************************************************************************************
@begin
  Result := False;
  var isAllOk : boolean;  isAllOk := True;
  do
  {
    var sMainAddr : string;  sMainAddr := 'ДанныеРаздельногоУчета';

    //========================================================================================================
    //#region Заголовок
    //--------------------------------------------------------------------------------------------------------
    do
    {
      SetStringValueByPath  (sMainAddr+'/ГенераторОтчета' , myGetVersProg);
      SetDateTimeValueByPath(sMainAddr+'/ДатаФормирования', _CurDateTime );
    }
    while False;
    if (not isAllOk)
    { Break; }
