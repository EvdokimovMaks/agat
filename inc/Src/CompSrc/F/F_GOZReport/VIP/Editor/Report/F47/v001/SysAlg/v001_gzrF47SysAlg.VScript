        // Аскерко1:
        // 1. не ставить проверку на положительный знак в строках: 1.2,  1.3,  1.4,  3.6,  4,  5.
        //    Здесь может быть как <+> так и <->
        // 2. в строках 2.3.7, 2.3.8 должен быть всегда минус
        // Аскерко2:
        // гр.3 по строкам 1.1, 1.2, 1.3, 1.4 считается наоборот, т.е. =гр.7- (гр. 4+5+6).
        // Таким образом, в этих строках то же будет положительное сальдо, кроме 1.4  (здесь может быть + и -).
        //----------------------------------------------------------------------------------------------------
        #undef   mCheckNegativeVar
        #declare mCheckNegativeVar(_var)
        sum#(_var) := GetDoubleValueByPath(path#(_var));
        sum#(_var) := GetDoubleValueByPath(path#(_var));
        if (sum#(_var) < 0)
        {
          pRLogRepEditor.AddWrnMessage(path#(_var), 'Отрицательное значение показателя');
        }
        #end   //mCheckNegativeVar
        //----------------------------------------------------------------------------------------------------
        #undef   mCheckPositiveVar
        #declare mCheckPositiveVar(_var)
        sum#(_var) := GetDoubleValueByPath(path#(_var));
        sum#(_var) := GetDoubleValueByPath(path#(_var));
        if (sum#(_var) > 0)
        {
          pRLogRepEditor.AddWrnMessage(path#(_var), 'Положительное значение показателя');
        }
