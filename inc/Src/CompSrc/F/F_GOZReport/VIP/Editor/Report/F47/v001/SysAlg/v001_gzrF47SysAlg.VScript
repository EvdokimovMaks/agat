//------------------------------------------------------------------------------------------------------------
// Установить видимость полей
@Script On_Visable : boolean;
@begin
  Result := False;
  var isAllOk : boolean;  isAllOk := True;
  do
  {
  }
  while False;
  Result := isAllOk;
@end. // On_Visable
//#endregion ДЕЙСТВИЯ ВЫПОЛНЯЕМЫЕ ПРИ УСТАНОВКЕ ВИДИМОСТИ ПОЛЕЙ
//************************************************************************************************************


//============================================================================================================
//#region ДЕЙСТВИЯ ВЫПОЛНЯЕМЫЕ ПРИ ПРОВЕРКЕ
//------------------------------------------------------------------------------------------------------------
// Проверить отчет
@Script On_Check : boolean;
  //==========================================================================================================
  //#region СЕРВИС
  //----------------------------------------------------------------------------------------------------------
  //#endregion СЕРВИС
  //**********************************************************************************************************
@begin
  Result := False;
  var isAllOk : boolean;  isAllOk := True;
  do
  {
    //========================================================================================================
    //#region УНИВЕРСАЛЬНАЯ ПРОВЕРКА
    var otRepStruct : IgzrTablRepStruct;  LoadVipRef(otRepStruct, 'gzrTablRepStruct');
    otRepStruct.SetSessionID(cRepSesID);
    do
    {
      _Loop otRepStruct
      {
        if (not otRepStruct.GetIsLeaf)
        { Continue; }
        if (InStr('xmlns', otRepStruct.Name) != 0)
        { Continue; }
        if (otRepStruct.sPath = '')
        {
          FpLog(FormatStr_2(
            'On_Check: (не задан _sPath): NRec=%S; "%S"'
          , string(otRepStruct.NRec)
          , otRepStruct.Name
          ) );
          Continue;
        }

        if (otRepStruct.wNeedMode = 1)
        { // если это обязательный для заполнения элемент
          if (not GetIsHidenRecInTreeByPath(otRepStruct.sPath) )
          { // если это НЕ скрытая запись в иерархии
            if (otRepStruct.sValue = '')
            { // если значение не определено
              pRLogRepEditor.AddErrMessage(otRepStruct.sPath, 'Не задано значение показателя');
            } // если значение не определено
          } // если это НЕ скрытая запись в иерархии
        } // если это обязательный для заполнения элемент

        if (GetIsRequiredInTreeByPath(otRepStruct.sPath) )
        { // если это обязательный элемент в иерархии
          if (otRepStruct.isHidenRec)
          { // если это скрытая запись
            pRLogRepEditor.AddErrMessage(otRepStruct.sPath, 'отсутствует обязательный элемент в XML');
          } // если это скрытая запись
        } // если это обязательный элемент в иерархии

      } // _Loop otRepStruct
    }
    while False;
    FreeVipInterface(otRepStruct);
    //#endregion УНИВЕРСАЛЬНАЯ ПРОВЕРКА
    //********************************************************************************************************

    //========================================================================================================
    //#region Контракты
    //--------------------------------------------------------------------------------------------------------
    do
    {
      var sMainAddr : string;  sMainAddr := 'ДанныеРаздельногоУчета';

      var sStdContractAddr : string ;  sStdContractAddr := sMainAddr+'/Контракт';
      var countContract    : LongInt;  countContract    := GetNodeCountByPath(sStdContractAddr);
      var indContract      : LongInt;
      for (indContract:=0; indContract<countContract; indContract++)
      { // побежали по Контрактам
        var sContractAddr : string;  sContractAddr := sStdContractAddr + GetIter(indContract);

        //----------------------------------------------------------------------------------------------------
        //#region объявление полей Контракта
        //----------------------------------------------------------------------------------------------------
        #undef   mDeclareRepVar
        #declare mDeclareRepVar(_var,_path)
        var sum#(_var) : double;  sum#(_var) := 0;  var path#(_var) : string;  path#(_var) := sUppAddr+#(_path);
        #end   //mDeclareRepVar
        //----------------------------------------------------------------------------------------------------

        //----------------------------------------------------------------------------------------------------
        //#region 1. Финансирование контракта (ГруппаФинансированиеКонтракта)
        var sUppAddr : string;  sUppAddr := sContractAddr+'/ГруппаФинансированиеКонтракта';

        #mDeclareRepVar(S1__G1, '/ЦелевойОбъемФинансирования'                                           )  // стр.1_гр.1
        #mDeclareRepVar(S1__G2, '/ПроцентВыполнения'                                                    )  // стр.1_гр.2
        #mDeclareRepVar(S1__G3, '/СальдоОпераций'                                                       )  // стр.1_гр.3

        #mDeclareRepVar(S11_G1, '/ДенежныеСредстваЗаказчика/ЦенаКонтракта'                              )  // стр.1.1_гр.1
        #mDeclareRepVar(S11_G2, '/ДенежныеСредстваЗаказчика/ПроцентВыполнения'                          )  // стр.1.1_гр.2
        #mDeclareRepVar(S11_G3, '/ДенежныеСредстваЗаказчика/СальдоОпераций'                             )  // стр.1.1_гр.3
        #mDeclareRepVar(S11_G4, '/ДенежныеСредстваЗаказчика/ВозвращеноЗаказчику'                        )  // стр.1.1_гр.4
        #mDeclareRepVar(S11_G6, '/ДенежныеСредстваЗаказчика/ВозвращеноЗаказчикуСобственныеСредства'     )  // стр.1.1_гр.6
        #mDeclareRepVar(S11_G7, '/ДенежныеСредстваЗаказчика/ПолученоОтЗаказчика'                        )  // стр.1.1_гр.7

        #mDeclareRepVar(S12_G1, '/БанковскиеКредиты/ПлановыйОбъемКредитования'                          )  // стр.1.2_гр.1
        #mDeclareRepVar(S12_G2, '/БанковскиеКредиты/ПроцентВыполнения'                                  )  // стр.1.2_гр.2
        #mDeclareRepVar(S12_G3, '/БанковскиеКредиты/СальдоОпераций'                                     )  // стр.1.2_гр.3
        #mDeclareRepVar(S12_G4, '/БанковскиеКредиты/ПогашеноТелаКредита'                                )  // стр.1.2_гр.4
        #mDeclareRepVar(S12_G6, '/БанковскиеКредиты/ПогашеноТелаКредитаСобственныеСредства'             )  // стр.1.2_гр.6
        #mDeclareRepVar(S12_G7, '/БанковскиеКредиты/ПривлеченоКредитов'                                 )  // стр.1.2_гр.7

        #mDeclareRepVar(S13_G3, '/ЗадолженностьПоПроцентамКредитов/СальдоОпераций'                      )  // стр.1.3_гр.3
        #mDeclareRepVar(S13_G4, '/ЗадолженностьПоПроцентамКредитов/ПогашеноПроцентов'                   )  // стр.1.3_гр.4
        #mDeclareRepVar(S13_G6, '/ЗадолженностьПоПроцентамКредитов/ПогашеноПроцентовСобственныеСредства')  // стр.1.3_гр.6
        #mDeclareRepVar(S13_G7, '/ЗадолженностьПоПроцентамКредитов/НачисленоПроцентов'                  )  // стр.1.3_гр.7

        #mDeclareRepVar(S14_G3, '/ЗадолженностьПоставщикам/СальдоОпераций'                              )  // стр.1.4_гр.3
        #mDeclareRepVar(S14_G4, '/ЗадолженностьПоставщикам/ОплаченоПоставщикам'                         )  // стр.1.4_гр.4
        #mDeclareRepVar(S14_G5, '/ЗадолженностьПоставщикам/ОплаченоПоставщикамСредстваДругихКонтрактов' )  // стр.1.4_гр.5
        #mDeclareRepVar(S14_G6, '/ЗадолженностьПоставщикам/ОплаченоПоставщикамСобственныеСредства'      )  // стр.1.4_гр.6
        #mDeclareRepVar(S14_G7, '/ЗадолженностьПоставщикам/СуммарнаяЗадолженность'                      )  // стр.1.4_гр.7
        //#endregion 1. Финансирование контракта (ГруппаФинансированиеКонтракта)
        //----------------------------------------------------------------------------------------------------

        //----------------------------------------------------------------------------------------------------
        //#region 2. Распределение ресурсов контракта (ГруппаРаспределениеРесурсовКонтракта)
        sUppAddr := sContractAddr+'/ГруппаРаспределениеРесурсовКонтракта';

        #mDeclareRepVar(S2__G3, '/СальдоОпераций' )  // стр.2_гр.3
        //#endregion 2. Распределение ресурсов контракта (ГруппаРаспределениеРесурсовКонтракта)
        //----------------------------------------------------------------------------------------------------

        //----------------------------------------------------------------------------------------------------
        //#region 2.1. Распределение ресурсов контракта (ГруппаРаспределениеРесурсовКонтракта/ГруппаДенежныеСредства)
        sUppAddr := sContractAddr+'/ГруппаРаспределениеРесурсовКонтракта/ГруппаДенежныеСредства';

        #mDeclareRepVar(S21_G3, '/СальдоОпераций'                                            )  // стр.2.1_гр.3
        #mDeclareRepVar(S21_G4, '/ДенежныеАктивы'                                            )  // стр.2.1_гр.4
        #mDeclareRepVar(S21_G5, '/ДенежныеАктивыСредстваДругихКонтрактов'                    )  // стр.2.1_гр.5
        #mDeclareRepVar(S21_G6, '/ДенежныеАктивыСобственныеСредства'                         )  // стр.2.1_гр.6
        #mDeclareRepVar(S21_G7, '/ИспользованиеРесурсов'                                     )  // стр.2.1_гр.7
        #mDeclareRepVar(S21_G8, '/ИспользованиеРесурсовДругиеКонтракты'                      )  // стр.2.1_гр.8
        #mDeclareRepVar(S21_G9, '/ИспользованиеРесурсовСобственныеСредства'                  )  // стр.2.1_гр.9

        #mDeclareRepVar(S211G3, '/ДенежныеСредстваОтдельныйСчет/СальдоОпераций'              )  // стр.2.1.1_гр.3
        #mDeclareRepVar(S211G4, '/ДенежныеСредстваОтдельныйСчет/ЗачисленоИсполнениеКонтракта')  // стр.2.1.1_гр.4
        #mDeclareRepVar(S211G6, '/ДенежныеСредстваОтдельныйСчет/ЗачисленоИное'               )  // стр.2.1.1_гр.6
        #mDeclareRepVar(S211G7, '/ДенежныеСредстваОтдельныйСчет/СписаноИсполнениеКонтракта'  )  // стр.2.1.1_гр.7
        #mDeclareRepVar(S211G8, '/ДенежныеСредстваОтдельныйСчет/СписаноДругиеКонтракты'      )  // стр.2.1.1_гр.8
        #mDeclareRepVar(S211G9, '/ДенежныеСредстваОтдельныйСчет/СписаноРасходыОрганизации'   )  // стр.2.1.1_гр.9

        #mDeclareRepVar(S212G3, '/БанковскиеДепозиты/СальдоОпераций'                         )  // стр.2.1.2_гр.3
        #mDeclareRepVar(S212G4, '/БанковскиеДепозиты/ПеречисленоНаДепозит'                   )  // стр.2.1.2_гр.4
        #mDeclareRepVar(S212G7, '/БанковскиеДепозиты/ВозвращеноСДепозита'                    )  // стр.2.1.2_гр.7

        #mDeclareRepVar(S213G3, '/АвансыВыданные/СальдоОпераций'                             )  // стр.2.1.3_гр.3
        #mDeclareRepVar(S213G4, '/АвансыВыданные/АвансыИсполнениеКонтракта'                  )  // стр.2.1.3_гр.4
        #mDeclareRepVar(S213G5, '/АвансыВыданные/АвансыСредстваДругихКонтрактов'             )  // стр.2.1.3_гр.5
        #mDeclareRepVar(S213G6, '/АвансыВыданные/АвансыСобственныеСредства'                  )  // стр.2.1.3_гр.6
        #mDeclareRepVar(S213G7, '/АвансыВыданные/ЗачтеноАвансов'                             )  // стр.2.1.3_гр.7
        #mDeclareRepVar(S213G9, '/АвансыВыданные/СписаноЗадолженностиКооперации'             )  // стр.2.1.3_гр.9
        //#endregion 2.1. Распределение ресурсов контракта (ГруппаРаспределениеРесурсовКонтракта/ГруппаДенежныеСредства)
        //----------------------------------------------------------------------------------------------------

        //----------------------------------------------------------------------------------------------------
        //#region 2.2. Запасы (ГруппаРаспределениеРесурсовКонтракта/ГруппаЗапасы)
        sUppAddr := sContractAddr+'/ГруппаРаспределениеРесурсовКонтракта/ГруппаЗапасы';

        #mDeclareRepVar(S22_G3, '/СальдоОпераций'                                                           )  // стр.2.2_гр.3
        #mDeclareRepVar(S22_G4, '/СформированоЗапасов'                                                      )  // стр.2.2_гр.4
        #mDeclareRepVar(S22_G5, '/СформированоЗапасовСредстваДругихКонтрактов'                              )  // стр.2.2_гр.5
        #mDeclareRepVar(S22_G6, '/СформированоЗапасовСобственныеСредства'                                   )  // стр.2.2_гр.6
        #mDeclareRepVar(S22_G7, '/ИспользованоЗапасов'                                                      )  // стр.2.2_гр.7
        #mDeclareRepVar(S22_G8, '/ИспользованоЗапасовНаДругиеКонтракты'                                     )  // стр.2.2_гр.8
        #mDeclareRepVar(S22_G9, '/ИспользованоЗапасовНуждыОрганизации'                                      )  // стр.2.2_гр.9

        #mDeclareRepVar(S221G3, '/МатериалыНаСкладах/СальдоОпераций'                                        )  // стр.2.2,1_гр.3
        #mDeclareRepVar(S221G4, '/МатериалыНаСкладах/ПоступилоМатериалов'                                   )  // стр.2.2,1_гр.4
        #mDeclareRepVar(S221G5, '/МатериалыНаСкладах/ПоступилоМатериаловСредстваДругихКонтрактов'           )  // стр.2.2,1_гр.5
