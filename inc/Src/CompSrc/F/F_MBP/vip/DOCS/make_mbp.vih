#ifndef __MAKE_MBP_VIH__
#define __MAKE_MBP_VIH__

#ifdef ComponentVersion
#Component "F_MBP"
#end

#doc
Функции для проведения накладной Склад -> Спецоснастка.<br>
#end
ObjInterface objMakeMBP;

  #doc
  Функция проверяет возможность создания
  приходов по накладной Склад -> Спецоснастка.<br>
  #end
  function ChkMakeOpers(pKatSopr : comp) : boolean;

  #doc
  Функция создает приходы по накладной Склад -> Спецоснастка.<br>
  #end
  function MakeOpers(pKatSopr : comp) : boolean;

  #doc
