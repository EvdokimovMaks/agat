#ifndef _ObjFpTForm_tbl_
#define _ObjFpTForm_tbl_

//------------------------------------------------------------------------------
Table struct FpMT_TFormTP
(
  cTForm       : comp,
  cTPer        : comp
)
with index
(
  FpMT_TFormTP01 = cTForm + cTPer
);

//------------------------------------------------------------------------------
Table struct FpMT_TFormKAU
(
  cTForm       : comp     // ссылка на типовую форму
, cFpKauTF     : comp     // ссылка на FpKauTF
, isSynch      : boolean  // требуется синхронизация
, isOnlyLeaf   : boolean  // добавлять только "листовые" значения
)
with index
(
  FpMT_TFormKAU01 = cTForm + cFpKauTF
);

#endif //_ObjFpTForm_tbl_
