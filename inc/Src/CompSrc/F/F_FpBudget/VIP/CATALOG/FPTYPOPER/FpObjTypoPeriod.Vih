#ifndef __OBJFPTYPOPERIOD_VIH__
#define __OBJFPTYPOPERIOD_VIH__

#ifdef  ComponentVersion
#component "F_FpBudget"
#endif//ComponentVersion

//------------------------------------------------------------------------------
#doc
объект периодов типологии периодов<br>
<b>Внимание!</b> Интерфейс предназначен только для внутреннего использования<br>
#end
#IntPrivate ObjInterface IObjFpTypoPeriod;

//==============================================================================
//                                  INIT
//******************************************************************************
//------------------------------------------------------------------------------
#doc
   Построить каталог периодов по всем типологиям указанного типа периода,<br>
   если каталог до этого уже был построен, но ничего не перестраивает<br>
#end
//------------------------------------------------------------------------------
function BuildAllCatalog(
  _cMainTPer : comp       // ссылка на тип периода
) : boolean;

//------------------------------------------------------------------------------
#doc
   Перестроить заново каталог периодов<br>
   по всем типологиям указанного типа периода<br>
#end
function ReBuildAllCatalog(
  _cMainTPer : comp       // ссылка на тип периода
) : boolean;

//------------------------------------------------------------------------------
#doc
   Загрузить каталог периодов по указанной типологии<br>
#end
//------------------------------------------------------------------------------
function LoadTypoPeriod(
  _cTypoTPer : comp       // ссылка на типологию периода
) : boolean;

//------------------------------------------------------------------------------
#doc
   Получить ближайший подчиненный период,<br>
   для синхронизации данных по текущей типологии<br>
#end
//------------------------------------------------------------------------------
function GetSubSynchPeriod(
  _cMainPeriod : comp
) : comp;

//==============================================================================
//                                 Field
//******************************************************************************
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
#doc
   Установить значения свойства по Nrec-у таблицы<br>
#end
function SetCatalogModify : boolean;

//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
end; // IObjFpTypoPeriod

VipInterface ObjFpTypoPeriod implements IObjFpTypoPeriod
#Licensed_Free
; // VipInterface ObjFpTypoPeriod

#endif  // __OBJFPTYPOPERIOD_VIH__



