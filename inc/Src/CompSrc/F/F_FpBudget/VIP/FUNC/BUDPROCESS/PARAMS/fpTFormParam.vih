#ifndef _ObjFpTFormParam_Included_
#define _ObjFpTFormParam_Included_

#ifdef  ComponentVersion
#component "F_FpBudget"
#endif//ComponentVersion

//------------------------------------------------------------------------------
#doc
Параметры типовых форм<br>
<b>Внимание!</b> Интерфейс предназначен только для внутреннего использования<br>
#end
#IntPrivate ObjInterface iFpTFormParam;

//------------------------------------------------------------------------------
#doc
   Получить в каком режиме задаются данные (0 - оба режима; 1 - одна ТФ; 2 - список ТФ)<br>
#end
function GetDataModeTForm : word; // режим данных  (1, N, Оба )

//------------------------------------------------------------------------------
#doc
   Получить значения единичного параметра ТФ
#end
function Get1TForm        : comp; // значение единичного параметра

//------------------------------------------------------------------------------
#doc
   Получить режим единичного параметра ТФ<br>
#end
function GetMode1TForm    : word; // что выбрано в роле А для 1

//------------------------------------------------------------------------------
#doc
   Получить режим параметра списка ТФ<br>
#end
function GetModeNTForm    : word; // что выбрано в роле А для N

//------------------------------------------------------------------------------
#doc
   Получить значение роли родителя для единичной ТФ
#end
function GetParent1TForm  : word; // значение роли родителя для 1 в роли А

//------------------------------------------------------------------------------
#doc
   Получить значение роли родителя для списка ТФ
#end
function GetParentNTForm  : word; // значение роли родителя для N в роли А

//------------------------------------------------------------------------------
#doc
   Проверка видимости полей<br>
#end
function CheckFieldState : boolean;

//------------------------------------------------------------------------------
#doc
   Выставить всем параметрам ссылку на родителя _wParent = 0 (Роль А) = 1 (Роль Б)
#end
function SetParentDefaults(_wParent : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Загрузить настройки интерфейса<br>
#end
function Read_ITune(_cTune : comp; _wEntity : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Сохранить настройки интерфейса<br>
#end
function Save_ITune(_cTune : comp; _wEntity : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Удалить настройки интерфейса<br>
#end
function Del_ITune(_cTune : comp; _wEntity : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Инициализация параметров интерфейса<br>
#end
function InitParam(
   _wModeN : word   // 0 - оба режима; 1 - один параметр; 2 - список параметров
): boolean;

//------------------------------------------------------------------------------
#doc
   Событие при открытии сценария модели<br>
#end
Event function Event_OpenScenario : boolean;

//------------------------------------------------------------------------------
#doc
   Событие при установке ссылки на родительские значения<br>
#end
Event function Event_SetParentDefaults : boolean;

//------------------------------------------------------------------------------
end;
//------------------------------------------------------------------------------

VipInterface FpTFormParam implements iFpTFormParam
#Licensed_Free
#ifdef  Atl51
Parameters (
  wModeN: word
)
#endif//Atl51
;

#endif  // _ObjFpTFormParam_Included_

