#ifndef __TRTablOSTSInfo_Vih__
#define __TRTablOSTSInfo_Vih__

#Component "F_TaxReport"

//==============================================================================
#doc
Табличный объект : "Информация об ТС из ОС"<br>
<b>Внимание!</b> Интерфейс предназначен только для внутреннего использования<br>
#end
Private ObjInterface ITRTablOSTSInfo;  //#region ObjInterface

  //============================================================================
  //#region ERROR
  //----------------------------------------------------------------------------
  #doc
     Получить описание последней ошибки выполнения
  #end
  function GetStLastError : string;
  //----------------------------------------------------------------------------
  #doc
     Вывести сообщения о последней ошибке выполнения
  #end
  function RunShowError : boolean;
  //#endregion ERROR
  //****************************************************************************


  //==============================================================================
  //#region FIELDS
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Ссылка на каталог льгот"
  #end
  function GetCKatLg : comp;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Код по ОКТМО"
  #end
  function GetOKTMO : string;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Код вида ТС"
  #end
  function GetKodVidTS : string;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Идентификационный номер ТС"
  #end
  function GetIdNomTS : TName;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Марка ТС"
  #end
  function GetMarkaTS : TName;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Регистрационный знак (номер) ТС"
  #end
  function GetRegZnakTS : TName;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Доля налогоплательщика в праве на ТС"
  #end
  function GetDoljaTS : TName;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Дата регистрации ТС"
  #end
  function GetDataRegTS : date;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Дата прекращения регистрации ТС (снятия с учета)"
  #end
  function GetDataSnRegTS : date;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "ОКЕИ"
  #end
  function GetOKEINalBaza : word;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Экологический класс"
  #end
  function GetJEkologKl : word;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Количество полных месяцев использования налоговой льготы"
  #end
  function GetLgotMesTS : word;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Налоговая база"
  #end
  function GetNalBaza : TSumma;
  //------------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Коэффициент Кп"
  #end
  function GetKojefKp : TSumma;
  //#endregion FIELDS
  //******************************************************************************



  //============================================================================
  //#region OBJECTTABLE
  //----------------------------------------------------------------------------
  property NRec   : comp       read;  // Номер записи
  property DTName : s200       read;  // Наименование типа документа
  property NoDoc  : TDocNumber read;  // Номер документа
  property dDoc   : date       read;  // Дата  документа
  property Name   : TName      read;  // Наименование
  property cKatOS : comp       read;  // Ссылка на ОС
  //----------------------------------------------------------------------------
  //index TRTablOSTSInfo01 = NRec(Unique, Primary);
  index TRTablOSTSInfo02 = Name;
  //#endregion OBJECTTABLE
  //****************************************************************************

End; //#endregion ObjInterface
