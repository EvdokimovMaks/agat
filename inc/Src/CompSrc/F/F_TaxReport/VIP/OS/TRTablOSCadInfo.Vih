#ifndef __TRTablOSCadInfo_Vih__
#define __TRTablOSCadInfo_Vih__

#Component "F_TaxReport"

//==============================================================================
#doc
Табличный объект : "Кадастровая информация по ОС"<br>
<b>Внимание!</b> Интерфейс предназначен только для внутреннего использования<br>
#end
Private ObjInterface ITRTablOSCadInfo;  //#region ObjInterface

  //============================================================================
  //#region ERROR
  //----------------------------------------------------------------------------
  #doc
     Получить описание последней ошибки выполнения
  #end
  function GetStLastError : string;
  //----------------------------------------------------------------------------
  #doc
     Вывести сообщения о последней ошибке выполнения
  #end
  function RunShowError : boolean;
  //#endregion ERROR
  //****************************************************************************


  //============================================================================
  //#region FIELDS
  //----------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Кадастровый номер"
  #end
  function GetCadNum     : TName;
  //----------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Кадастровый номер помещения"
  #end
  function GetCadNumAdd  : TName;

  //----------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Кадастровая стоимость"
  #end
  function GetStoim      : TSumma;

  //----------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Категория земель"
  #end
  function GetCatZem     : TName;

  //----------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Налоговая ставка (%)"
  #end
  function GetNalStav    : TSumma;

  //----------------------------------------------------------------------------
  #doc
     Получить строковое значение атрибута "Доля налогоплательщика (числитель)"
  #end
  function GetStrDoljaNPChsl : string;
  //----------------------------------------------------------------------------
  #doc
     Получить строковое значение атрибута "Доля налогоплательщика (знаменатель)"
  #end
  function GetStrDoljaNPZnam : string;

  //----------------------------------------------------------------------------
  #doc
     Получить значение атрибута "Не является объектом налогообложения для земельного налога"
  #end
  function GetIsNotObjTax : boolean;
  //#endregion FIELDS
  //****************************************************************************


  //============================================================================
  //#region OBJECTTABLE
  //----------------------------------------------------------------------------
  property NRec        : comp         read;  // Номер записи
  property NoDoc       : TDocNumber   read;  // Номер документа
  property dDoc        : date         read;  // Дата  документа
  property Name        : TName        read;  // Наименование
  property cKatOS      : comp         read;  // Ссылка на ОС
  //----------------------------------------------------------------------------
  //index TRTablOSCadInfo01 = NRec(Unique, Primary);
  index TRTablOSCadInfo02 = Name;
  //#endregion OBJECTTABLE
  //****************************************************************************

End; //#endregion ObjInterface
//******************************************************************************

