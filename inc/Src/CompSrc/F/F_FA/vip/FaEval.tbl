#ifndef _FaEval_Included_
#define _FaEval_Included_

#Component "F_FA"

table struct FaEval
(
  Nrec                 : tNrec,
  cCO                  : tNRec      "Ссылка на ЦО",
  CellErr              : word       "Ошибки в адресации",
  cGrCO                : tNRec      "Ссылка на группу ЦО",
  cGrIndex             : tNRec      "Ссылка на группу индексов",
  compValue            : comp       "Значение типа comp",
  cOutCycle            : tNRec      "Проверка цикличности",
  cPeriod              : tNRec      "Ссылка на период",
  cVal                 : tNRec      "Ссылка на валюту",
  cLegend              : tNRec      "Ссылка на легенду",
  Formula              : tExpr      "Формула",
  FormulErr            : word       "Ошибки в формулах",
  IdType               : word       "Идентификатор типа",
  isCorrect            : boolean    "Флаг корректности",
  lngValue             : longint    "Целое значение, дата, время",
  NameCell             : s80        "Имя ячейки",
  NumCell              : s6         "Номер ячейки",
  strValue             : tExpr      "Строковое значение",
  Value                : double     "Значение",
  ValueVal             : double     "Значение в валюте"
)
With Index
(
  FaEval01 = NRec (Unique, Surrogate),
  FaEval02 = NumCell,
  FaEval03 = cOutCycle,
  FaEval04 = NameCell,
  FaEval05 = FormulErr,
  FaEval06 = CellErr,
  FaEval07 = isCorrect
