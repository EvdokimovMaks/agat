.{
.begin
  for (Хар1 := 1; Хар1 <= Count(Характеристика_Наименование_Объекта); Хар1 := Хар1 + 1)
  {
    Характеристика_Наименование_Объекта [Хар1] := '';
    Характеристика_Количество_Объектов  [Хар1] := '';
  }
  for (Хар2 := 1; Хар2 <= Count(Характеристика_NameDrag); Хар2 := Хар2 + 1)
  {
    Характеристика_NameDrag     [Хар2] := '';
    Характеристика_NomenclaturN [Хар2] := '';
    Характеристика_N_Edin       [Хар2] := '';
    Характеристика_MassaDrag    [Хар2] := '';
  }
  Хар1 := 0;
  Хар2 := 0;
  ХарN := 0;

  Поступление_Новый_Объект := False;
  if (Word(Тип_Операции) = 1)
    if ((Срок_использования_до_поступления = 0.0) AND (Новый_износ_опер = 0.0))
      Поступление_Новый_Объект := True;
end.
