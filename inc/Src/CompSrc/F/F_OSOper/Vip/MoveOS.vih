#ifndef __MoveOs__vih__
#define __MoveOs__vih__

#include TDocument.vih

#component "F_OSOPER"


//-----------------------------------------------------------------------------
ObjInterface ObjMoveOs;
  #doc
  Функция получения буфера MoveOs <br>
  </brief>
  #end
  function GetMoveOsBuff  : type$MoveOs;
  #doc
  Функция установки буфера MoveOs <br>
  </brief>
  #end
  procedure SetMoveOsBuff(MoveOsBuffer: type$MoveOs);
end;

ObjInterface ObjMoveOsEx;
  #doc
  Функция запуска с доп. параметрами.
  </brief>
  #end
  Function RunWithParams(_pVidOp, _wTip: Word;
                         _N_KatOS: Comp;
                         Var _N_MoveOs: Comp;
                         _bPosKatOs: Boolean;
                         _PrVbr: Word;
                         _BegDate, _EndDate: Date) : Word;
end;

//-----------------------------------------------------------------------------
#doc
Точка расширения перед вставкой записи в MoveOs.<br>
</brief>
  Параметры:
<ul>
  <li> pMoveOs:ObjMoveOs  - ссылка на текущий интерфейс;
</ul>
#end
ExtensionPoint epOnMoveOsInsert(pMoveOs: ObjMoveOs);

//-----------------------------------------------------------------------------
#doc
Точка расширения перед обработкой события cmPick.<br>
Если возвращаемый результат true, то вызывается стандартная обработка, иначе обработка cmPick прерывается
</brief>
  Параметры:
<ul>
  <li> pMoveOs:ObjMoveOs  - ссылка на текущий интерфейс;
  <li> sFieldName: string - имя поля для которого произошло событие.
</ul>
#end
ExtensionPoint epOnMoveOsPick(pMoveOs: ObjMoveOs; sFieldName: string);



VipInterface MoveOs implements TDocument, ObjMoveOs, ObjMoveOsEx
  Licensed
  (
#include MoveOs.lih
  )
  Parameters (pVidop, wTip: word; N_KatOs, N_MoveOs: comp; bPosKatOs: boolean; PrVbr : word);

#end // __MoveOs__vih__
