  //------------------------------     итого
  BeginUpCase(NumToString(Итого_общ_кол_номеров_объектов, ''))
  if (Есть_операция_инвентаризации,   BeginUpCase(NumToString(TotalKolFact, '')),  '')
  Валюта_метода
  if (Есть_операция_инвентаризации, BeginUpCase(DoubleToString(0, TotalSummaFact)), '')
.endfields
