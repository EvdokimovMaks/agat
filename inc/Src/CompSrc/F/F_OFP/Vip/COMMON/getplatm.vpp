////////////////////////////////////////////////////////////////////////////////
// размер для массивов списка
// (изп.только в mFill_MeanType)
Function List_GetSize(_type: word): word;
var ret: word;
{
  case _type of
    cgOfpMT_FopTrans:
       ret := 6;
    cgOfpMT_FopVzaim:
       ret := 1;
    cgOfpMT_FopConvt:
       ret := 5;
    cgOfpMT_FopConvtDnForKassa:
       ret := 3;
    cgOfpMT_FopConvtDnForVekCb:
       ret := 2;
    cgOfpMT_LicvHigh:
       ret := 4;
    cgOfpMT_LicvLow:
       ret := 5;
    cgOfpMT_All:
       ret := 7;
  end;

  List_GetSize := ret;
}
// заполнение списков ПС
#undef mFill_MeanType
#declare mFill_MeanType(_Type,_arrS,_arrW)
{
  var curSize: word;
  curSize := List_GetSize(#_type);

  SetLimit(#_arrS, curSize);
  SetLimit(#_arrW, curSize);

  case #_Type of
    cgOfpMT_FopTrans:
    {
      #_arrW[1] := mtKASSA;
      #_arrW[2] := mtACCOUNT;
      #_arrW[3] := mtACCOUNT_VAL;
      #_arrW[4] := mtSECURITY;
      #_arrW[5] := mtVEKSL;
      #_arrW[6] := mtAVISO;
     }
    cgOfpMT_FopVzaim:
    {
      #_arrW[1] := mtZADOLJ;
    }
    cgOfpMT_FopConvt:
    {
      #_arrW[1] := mtKASSA;
      #_arrW[2] := mtACCOUNT;
      #_arrW[3] := mtACCOUNT_VAL;
      #_arrW[4] := mtSECURITY;
      #_arrW[5] := mtVEKSL;
    }
    cgOfpMT_FopConvtDnForKassa: // вверху касса
    {
      #_arrW[1] := mtKASSA;
      #_arrW[2] := mtACCOUNT;
      #_arrW[3] := mtACCOUNT_VAL;
    }
    cgOfpMT_FopConvtDnForVekCb: // // вверху вексель или ц/б
    {
      #_arrW[1] := mtACCOUNT;
      #_arrW[2] := mtACCOUNT_VAL;
    }
    cgOfpMT_LicvHigh : // ПС высокой ликвидности
    {
      #_arrW[1] := mtNOTDEFINED;
      #_arrW[2] := mtKASSA;
      #_arrW[3] := mtACCOUNT;
      #_arrW[4] := mtACCOUNT_VAL;
    }
    cgOfpMT_LicvLow : // ПС низкой ликвидности
    {
      #_arrW[1] := mtNOTDEFINED;
      #_arrW[2] := mtSECURITY;
      #_arrW[3] := mtVEKSL;
      #_arrW[4] := mtZADOLJ;
      #_arrW[5] := mtAVISO;
    }
    cgOfpMT_All : // все ПС - для фильтра
    {
      #_arrW[1] := mtKASSA;
      #_arrW[2] := mtACCOUNT;
      #_arrW[3] := mtACCOUNT_VAL;
      #_arrW[4] := mtSECURITY;
      #_arrW[5] := mtVEKSL;
      #_arrW[6] := mtZADOLJ;
      #_arrW[7] := mtAVISO;
    }
  end;

  var i: word;
  for (i:=1; i<=curSize; i:=i+1)
    #_arrS[i] := piClearing.Clearing_GetMeanTypeName(#_arrW[i], False);
}
#end //mFill_MeanType
