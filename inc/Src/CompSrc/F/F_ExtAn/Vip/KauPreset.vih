#ifndef __KauPreset_vih__
#define __KauPreset_vih__

#ifdef ComponentVersion
#Component "F_ExtAn"
#end

#doc
Объект для обработки и редактирования набора значений аналитики
#end
ObjInterface ObjKauPreset;
  #doc
  очистить набор
  #end
  procedure ClearPreset;

  #doc
  получить значение аналитики
  #end
  function GetPreset(wKau : word) : comp;

  #doc
  установить значение аналитики
  #end
  function SetPreset(wKau : word; cKau : comp) : boolean;

  #doc
  вызвать окно редактирования набора аналитики
  #end
  function ExecPreset(CanEdit : boolean) : word;

  #doc
  получить строку для высветки аналитики <br>
  num - константа атрибут аналитики см. Tip&Stat\KauTable.xls (например cgAnName)
  #end
  function ShowPreset(num : word) : string;

  #doc
  копирование набора во внешние КАУ
  #end
  function CopyPreset2KauReff(coTable: word;  cRec: comp) : boolean;

  #doc
  копирование внешних КАУ в набор
  #end
  function CopyKauReff2Preset(coTable: word;  cRec: comp) : boolean;

  #doc
  копирование набора в контейнер аналитики
  #end

  function CopyPreset2ContAn(ContAn: longint) : boolean;

  #doc
  копирование контейнера аналитики в набор
  #end
  function CopyContAn2Preset(ContAn: longint) : boolean;
End;

VipInterface KauPreset
  Implements ObjKauPreset
  #ifdef Atl51
  Licensed (Free)
  #end
;

#endif //__KauPreset_vih__
