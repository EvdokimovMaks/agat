#ifndef __FORMOS6_VIH__
#define __FORMOS6_VIH__

#Component "F_OSInvCard"

#doc
  Объект печати инвентарной карточки модуля ОС (НМА).<br>
#end
ObjInterface ObjFormOS6;

#doc
  Печать инвентарной карточки (форма N ОС-6)
#end
  // ---------------------------------------------------------------------------
  // Передаваемые параметры:
  //    pwTiDk         - тип объектов учета ( ОС/НМА )
  //    pcKatOsNRec    - ссылка на карточку для печати
  //    pcNastrOSNRec  - метод учета
