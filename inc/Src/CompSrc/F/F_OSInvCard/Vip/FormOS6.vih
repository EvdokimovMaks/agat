  //    areMarked      - печатаем по помеченным записям или нет
  // ---------------------------------------------------------------------------
  Procedure PrintFormOS6( pwTiDk : word;
                          pcKatOsNRec, pcNastrOsNRec, pcPlansSchNRec : comp;
                          pdOtchPer : date;
                          areMarked : boolean );
#doc
  Параметры для печати инвентарной карточки (форма N ОС-6)
#end
  Function GetParamFormOS6 : boolean;
end;

VipInterface FormOS6  Implements ObjFormOS6  Licensed  (Free)
  Parameters (wTiDk : word; cKatOsNRec, cNastrOsNRec, cPlansSchNRec : comp);

