#ifndef _ANALPICK_VIH_
#define _ANALPICK_VIH_

#ifdef ComponentVersion
#Component "F_TXO"
#end

#doc
Объект для выбора аналитики или другого признака
#end
ObjInterface ObjAnalPick;
  #doc
  добавить элемент в список выбора
  #end
  function Add(Code : word; Name : string) : boolean;

  #doc
  удалить элемент из списка выбора
  #end
  function Del(Code : word) : boolean;

  #doc
  открыть окно выбора, возвращает код выбранного элемента, результат FALSE если нажали Esc
  #end
  function ExecPick(var Code : word) : boolean;
end;

VipInterface IAnalPick
  Implements ObjAnalPick
  #ifdef Atl51
  Licensed (Free)
  #end
;

#end // _ANALPICK_VIH_
