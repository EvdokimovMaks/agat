#ifndef __OSTablSpDINalImush_Vih__
#define __OSTablSpDINalImush_Vih__

#component "F_Common"

//------------------------------------------------------------------------------
#include IModify.Vih       // Абстрактный табличный интерфейс
#include IManualIndex.Vih  // Абстрактный интерфейс для индексов типа Manual
//------------------------------------------------------------------------------

//==============================================================================
#doc
Табличный объект : "Спецификация документа Налог на имущество"<br>
<b>Внимание!</b> Интерфейс предназначен только для использования внутри "Бухгалтерского контура"<br>
#end
ObjInterface IOSTablSpDINalImush;  //#region ObjInterface

  //----------------------------------------------------------------------------
  #doc
     Получить описание последней ошибки выполнения
  #end
  function GetStLastError : string;
  //----------------------------------------------------------------------------
  #doc
     Вывести сообщения о последней ошибке выполнения
  #end
  function RunShowError : boolean;


  //----------------------------------------------------------------------------
  //#region Property
  Property NRec        : comp    read write;  // Номер записи
  Property cDocInfo    : comp    read write;  // DocInfo.NRec
  Property DocType     : word    read write;  // Тип документа

  Property IsImNotTax  : boolean read write;  // Не подлежит налогообложению
  Property wImMovable  : word    read write;  // Тип имущества (движимое/недвижимое)
  Property wImTaxBase  : word    read write;  // Налоговая база недвижимого имущества (среднегодовая/кадастровая стоимость)
  Property ImNotTaxSum : TSumma  read write;  // Не облагаемая кадастровая стоимость
  Property wImVidCode  : word    read write;  // Код вида имущества
  Property cImKatRate  : comp    read write;  // ссылка на "Каталог ставок и льгот"
  Property wImFeatures : word    read write;  // Особенности налогообложения
  //#endregion Property
  //----------------------------------------------------------------------------

  //----------------------------------------------------------------------------
  //#region Index
  Index OSTablSpDINalImush01 = NRec(Unique, Primary);
  Index OSTablSpDINalImush02 = cDocInfo, DocType;
  //#endregion Index
  //----------------------------------------------------------------------------

End; //#endregion ObjInterface
//******************************************************************************

//==============================================================================
VipInterface OSTablSpDINalImush implements IOSTablSpDINalImush, IModify, IManualIndex
Licensed(Free);
Public: //#region Public
  constructor Init;
  destructor  Done;

  function  IModify.doInsert : word;
  function  IModify.doUpdate : word;
  function  IModify.doDelete : word;

  function  IManualIndex.Count : LongInt;
End; //#endregion Public
//******************************************************************************

#endif//__OSTablSpDINalImush_Vih__
