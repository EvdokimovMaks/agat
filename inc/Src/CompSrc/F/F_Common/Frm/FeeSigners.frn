!-------------------------------------------------------------------------------
! Макроопределение для вставки в секции вывода данных присоединенной формы
#declare FeeSignersLinkInit(FormName)
.begin
  // *** Обнуление переменной ***
  Номер_подписанта := 0;

  // *** Обнуление массивов по таблице ***
  for (Номер_подписанта := 1; Номер_подписанта <= Count(Массив_Подписант_nRec); Номер_подписанта := Номер_подписанта + 1)
  {
    Массив_Подписант_nRec            [Номер_подписанта] := string(0);
    Массив_Подписант_ФИО             [Номер_подписанта] := '';
    Массив_Подписант_Табельный_Номер [Номер_подписанта] := ''; // LongInt(0)
    Массив_Подписант_Должность       [Номер_подписанта] := '';
    Массив_Подписант_Роль            [Номер_подписанта] := '';
  }

  Номер_подписанта := 0;
end.
.{ CheckEnter FeeSignersMember_#FormName
