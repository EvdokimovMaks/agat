   Summa3:=Summa3 + (Количество_всего_фактическое1 * (цена_первая +  (цена_первая * оптовая_надбавка)/100));
   Summa4:=Summa4 + сумма_вход_НДС1;
   Summa5:=Summa5 + (Количество_всего_фактическое1 * (цена_первая +  (цена_первая * оптовая_надбавка)/100) + сумма_вход_НДС1);
   Summa6:=Summa6 + ((цена_первая * Количество_всего_фактическое1) * (розничная_надбавка / 100));
   Summa7:=Summa7 + сумма_исход_НДС1;
   Summa9:=Summa9 + Round((( (Количество_всего_фактическое1 * (цена_первая +  (цена_первая * оптовая_надбавка)/100)) + 
                             ((цена_первая * Количество_всего_фактическое1) * (розничная_надбавка / 100)) +
                             сумма_исход_НДС1)), 2);
   Summa10:=Summa10 +  Round((Количество_всего_фактическое1 * (цена_первая +  (цена_первая * оптовая_надбавка)/100)) + 
                             ((цена_первая * Количество_всего_фактическое1) * (розничная_надбавка / 100)) +
                             сумма_исход_НДС1 + (цена_розничная * (налог_с_продажи / 100)), 2) -  
                       Round((( (Количество_всего_фактическое1 * (цена_первая +  (цена_первая * оптовая_надбавка)/100)) + 
                                ((цена_первая * Количество_всего_фактическое1) * (розничная_надбавка / 100)) +
                                 сумма_исход_НДС1 + (цена_розничная * (налог_с_продажи / 100)))), 2);
end.
