#ifndef _XKatBank_vih_INCLUDED_
#define _XKatBank_vih_INCLUDED_

#component "L_KatOrg"

objinterface IXKatBank;
  #doc
    Загрузить информацию по ссылке на банк
  #end
  function Seek(cPrimaryBank: comp): boolean;

  #doc
    Организация, связанная с загруженным банком
  #end
  function PrimaryOrg: comp;

  #doc
    Функциональная роль загруженного банка
  #end
  function GetAktiv: word;

  #doc
    Используемый по умолчанию банк для уплаты банковского сбора
  #end
  function GetSpecialBank(var cBank: comp): boolean;

  #doc
    У загруженного банка нет дополнительной связи с банком
  #end
  function SecondaryIsEmpty: boolean;

  #doc
    Возвращает ссылку на банк контрагента
  #end
  function GetSecondaryBank: comp;

  #doc
    Возвращает ссылку на контрагента, связанного с банком
  #end
  function GetSecondaryOrg: comp;

end;

vipinterface XKatBank
  implements IXKatBank
  licensed (free)
  parameters (cOrganization, cBank: comp)
;

#end
