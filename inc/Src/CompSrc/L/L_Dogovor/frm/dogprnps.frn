#doc
Закупка
Паспорт импортной сделки для Казахстана
#end
.LinkForm 'DogovorReport_03' Prototype is 'DogPrn2'
.NameInList 'Паспорт импортной сделки для Казахстана'
.group 'Закупка'
.Group 'Country' subGroup 'Kazakhstan'
.create view loBD
var
  cDogovor: comp;
  PosrName: string;
  IsPosr  : boolean;
  oHistory: iHistory;
from
  Dogovor
, SpDocs
, KatMC
, KatUsl
, KlVal
, Persons
, Appointments
, Passports
, AttrDog
, KatOrg   Pol
, KatOrg   Post
, KatOrg   Posr
, KatCity  CityPol
, KatCity  CityPost
, KatCity  CityPosr
, KatState StatePost
, KatBank  BankPlat
, KatBank  BankPol
, KatBank  BankPosr
, KlVal    KlValRas
where
((
                cDogovor == Dogovor.NRec
and         Dogovor.TiDk == SpDocs.TiDk
and         Dogovor.NRec == SpDocs.cDoc
and        SpDocs.cMCUsl == KatMC.NRec
and        SpDocs.cMCUsl == KatUsl.NRec
and         Dogovor.nRec == AttrDog.cDogovor
and    coGetTune('Boss') == Persons.nRec
and     Persons.PasspRus == Passports.nRec
and         Dogovor.cVal == KlVal.NRec
and      Dogovor.cValRas == KlValRas.NRec
and         Dogovor.cOrg == Post.NRec
and       Dogovor.cMyOrg == Pol.NRec
and           Post.cCity == CityPost.NRec
and            Pol.cCity == CityPol.NRec
and             PosrName == Posr.Name
and          Post.cState == StatePost.NRec
and           Posr.cCity == CityPosr.NRec
and    Dogovor.cBankPlat == BankPlat.NRec
and     Dogovor.cBankPol == BankPol.NRec
))
;
.var
  PSINum       : string
  PSIDate      : date
  Tovar
, FormRasc     : string
  BankPlatRNN
, BankPlatOKPO : string
  OplTov       : string
  NalOrgAddrImp: string
  NalOrgAddr3  : string
  SrokOpl      : string
  ValOgovorka  : string
  BankNotes    : string
.endvar
.begin
  loBD.cDogovor := nRecД_С;

  if (loBD.GetFirst Post    = 0) {}
  if (loBD.GetFirst Pol     = 0) {}
  if (loBD.GetFirst Persons = 0) {}
  if (loBD.GetFirst Dogovor = 0)
  {
    var ExtAttr  : IExtAttr;
    var ExClass  : IExtClass;
    var cAttrNam : comp;
    var wTypeAttr: word;

    BankPlatRNN   := ExtAttr.sGetAttr(coKatBank, loBD.Dogovor.cBankPlat, 'РНН'                    );
    BankPlatOKPO  := ExtAttr.sGetAttr(coKatBank, loBD.Dogovor.cBankPlat, 'ОКПО'                   );
    NalOrgAddrImp := ExtAttr.sGetAttr(coKatOrg , loBD.Dogovor.cMyOrg   , 'Адрес налогового органа');
    PosrName      := ExtAttr.sGetAttr(coDogovor, nRecД_С               , 'Третье лицо'            );

    IsPosr := (loBD.GetFirst Posr = 0);

    if IsPosr
    {
      NalOrgAddr3 := ExtAttr.sGetAttr(coKatOrg, loBD.Posr.NRec, 'Адрес налогового органа');

      if (loBD.GetFirst BankPosr where (( loBD.Posr.NRec == BankPosr.cOrg and
                                                 word(1) == BankPosr.Aktiv )) <> 0)
        if (loBD.GetFirst BankPosr where (( loBD.Posr.NRec == BankPosr.cOrg )) = 0) {}
    }

    // ПС
    Tovar := '';

    loBD._loop SpDocs
      Tovar += if(Tovar = '', '', ',') + if ((loBD.SpDocs.PrMc = 2) or (loBD.SpDocs.PrMc = 3), KatUsl.Name, KatMC.Name);

    PSINum      := ExtAttr.sGetAttr(coDogovor, nRecД_С, 'Паспорт сделки (ПС)');
    // Валютная оговорка
    ValOgovorka := ExtAttr.sGetAttr(coDogovor, nRecД_С, 'Валютная оговорка');
    // Особые отметки Банка, Таможенного органа
    BankNotes   := ExtAttr.sGetAttr(coDogovor, nRecД_С, 'Особые отметки Банка, Таможенного органа');
    // дата регистрации ПС
    PSIDate     := ExtAttr.dGetAttr(coDogovor, nRecД_С, 'Дата регистрации ПС');

    //форма расчетов
    wTypeAttr := ExClass.GetClassCode(coDogovor, 'Форма расчетов');

    if (wTypeAttr = 0)
      FormRasc := ''
    else
      FormRasc := ExClass.GetSegName(wTypeAttr, nRecД_С);

    //оплата товара
    wTypeAttr := ExClass.GetClassCode(coDogovor, 'Оплата товара');

    if (wTypeAttr = 0)
      OplTov := ''
    else
      OplTov := ExClass.GetSegName(wTypeAttr, nRecД_С);
  };
end.
.fields
!  ПАСПОРТ СДЕЛКИ ПО ИМПОРТУ №  Б^ Б от  Б^ Б
  PSINum
  if (longint(PSIDate) = 0, '', DateToStr(PSIDate, 'DD/MM/YYYY г.'))

!  Реквизиты банка:
!  Наименование -                    Б^ Б
!  Филиал -
!  Адрес -                           Б^ Б
!  РНН -                             Б^ Б
!  Код ОКПО банка (филиала банка) -  Б^ Б
  loBD.BankPlat.Name
  loBD.BankPlat.Addr
  GetKatOrgAttrName4Rep('UNN', 0, GetStateRef(ccKaz))
  BankPlatRNN
  GetKatOrgAttrName4Rep('OKPO', 0, GetStateRef(ccKaz))
  BankPlatOKPO

!  Реквизиты импортера:
!  Наименование -                    Б^ Б
!  Код ОКПО -                        Б^ Б
!  Адрес -                           Б^ Б
!  Номер банковского счета -         Б^ Б
!  РНН -                             Б^ Б
!  Адрес налогового органа -         Б^ Б
  oHistory.sGetField(coKatOrg, loBD.Pol.nRec, 'NAME', loBD.Dogovor.dDoc)
  GetKatOrgAttrName4Rep('OKPO', loBD.Pol.nRec)
  loBD.Pol.OKPO
  loBD.Pol.Addr+', '+loBD.CityPol.Name
  loBD.BankPlat.Schet1
  GetKatOrgAttrName4Rep('UNN', loBD.Pol.nRec)
  loBD.Pol.UNN
  NalOrgAddrImp

!  Реквизиты третьего лица:
!  Наименование -                    Б^ Б
!  Код ОКПО -                        Б^ Б
!  Адрес -                           Б^ Б
!  Номер банковского счета -         Б^ Б
!  РНН -                             Б^ Б
!  Адрес налогового органа -         Б^ Б
  if(not IsPosr, '', oHistory.sGetField(coKatOrg, loBD.Posr.nRec, 'NAME', loBD.Dogovor.dDoc))
  if(not IsPosr, GetKatOrgAttrName4Rep('OKPO', 0, GetStateRef(ccKaz)), GetKatOrgAttrName4Rep('OKPO', loBD.Posr.nRec))
  if(not IsPosr, '', loBD.Posr.OKPO)
  if(not IsPosr, '', loBD.Posr.Addr+', '+loBD.CityPosr.Name)
  if(not IsPosr, '', loBD.BankPosr.Schet1)
  if(not IsPosr, GetKatOrgAttrName4Rep('UNN', 0, GetStateRef(ccKaz)), GetKatOrgAttrName4Rep('UNN', loBD.Posr.nRec))
  if(not IsPosr, '', loBD.Posr.UNN)

  NalOrgAddr3

!  Реквизиты инопартнера:
!  Наименование -                    Б^ Б
!  Страна -                          Б^ Б
!  Адрес -                           Б^ Б
!  Реквизиты банка инопартнера -     Б^ Б
  oHistory.sGetField(coKatOrg, loBD.Post.nRec, 'NAME', loBD.Dogovor.dDoc)
  loBD.StatePost.Name
  loBD.Post.Addr+', '+loBD.CityPost.Name
  loBD.BankPol.Name+
    if(loBD.BankPol.Addr   = '', '', ', '   +loBD.BankPol.Addr)+
    if(loBD.BankPol.Schet1 = '', '', ', р/с'+loBD.BankPol.Schet1)

!  Реквизиты и условия контракта:
!  Номер -                    Б@@@@@@@@@@@@@@@@@@ Б   Последняя дата -      Б^ Б
!  Дата -                     Б@@@@@@@@@@@@@@@@@@ Б   Валюта платежа -      Б^ Б
!  Сумма контракта -          Б&'&&&&&&&&&&&&&.&& Б   Код валюты платежа -  Б^ Б
!  Товар -                    Б@@@@@@@@@@@@@@@@@@ Б   Форма расчетов -      Б^ Б
!  Валюта по контракту -      Б@@@@@@@@@@@@@@@@@@ Б
!  Код валюты по контракту -  Б@@@@@@@@@@@@@@@@@@ Б
!  Валютная оговорка -        Б^ Б
  loBD.Dogovor.NoDoc DateToStr(loBD.Dogovor.dEnd, 'DD/MM/YYYY')
  loBD.Dogovor.dDoc  if(loBD.Dogovor.cValRas = 0, sGetTune('NDE.SimvRub'), loBD.KlValRas.Dollar)
  loBD.Dogovor.Summa if(loBD.Dogovor.cValRas = 0, sGetTune('NDE.RublISO'), loBD.KlValRas.ISO)
  Tovar FormRasc
  if(loBD.Dogovor.cVal = 0, sGetTune('NDE.SimvRub'), loBD.KlVal.Dollar)
  if(loBD.Dogovor.cVal = 0, sGetTune('NDE.RublISO'), loBD.KlVal.ISO)
  ValOgovorka

  ''  AttrDog.Sign_My_Post  ''
  ''  AttrDog.Sign_My_Fio   ''
  ''  ''
  ''  ''
  BankNotes
.endfields

.{ CheckEnter DogGroup
.}


