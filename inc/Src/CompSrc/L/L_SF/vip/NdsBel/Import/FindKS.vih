  Установка критериев поиска
  #end
  Procedure SetParam(pTuneImport: TImportInvoiceTune);

  #doc
  Поиск первичных документов
  #end
  Function FindSHFromInvoice(cSF: comp; isPack: boolean = false; wMode: word = word(0)): comp;

End;

VipInterface FindKS
