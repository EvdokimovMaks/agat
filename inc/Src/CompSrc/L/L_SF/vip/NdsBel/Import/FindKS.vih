#ifndef __FindKS_vih__
#define __FindKS_vih__

#Component "L_SF"

Type
  TImportInvoiceTune = record
    wParam   : word;
    wTypeDoc : word;
    wSF      : word;
    wPrimDoc : word;
  end;

#doc
Поиск сопроводительных документов
#end
ObjInterface ObjFindKS;

  #doc
  Установка критериев поиска
  #end
