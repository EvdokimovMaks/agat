Table STRUCT MTabReestrforKN "Таблица в памяти"
  (
    NRec          : comp,
    Pick          : word,
    isReadOnly    : boolean,
    KatSoprnRec   : comp,
    KatSoprVidSopr: word,
    SpSoprnRec    : comp,
    SpSoprNpp     : tNpp,
    KatSoprnSopr  : string,
    KatSoprdSopr  : Date,
    KatSoprdOpr   : Date,
    KatSoprcVal   : comp,
    KatSoprVhodNal: word,
    KatSoprSkid   : double,
    KatMCname     : string,
    cKatMc        : comp,
    cParty        : comp,
    KatPodrName   : string,
    KatMolName    : string,
    cKatPodr      : comp,
    cKatMol       : comp,
    KatPartyKOD   : string,
    KatPartyname  : string,
    KatOrgname    : string,
    OldPrice      : double,
    NewPrice      : double,
    KolOpl        : double,
    NewKolOpl     : double,
    PodrFrom      : string,
    MolFrom       : string,
    wADocSp       : word,
    cADocSp       : comp,
    SumPos        : double
  )
With Index
  (
      TMP00indexmem = NRec(Unique, Surrogate, Journal)
    , TMP01indexmem = KatSoprnSopr
    , TMP02indexmem = KatSoprdSopr
    , TMP03indexmem = KatMCName
    , TMP04indexmem = KatPartyName + KatPodrName + KatMolName
    , TMP041indexmem = KatPartyKOD + KatPodrName + KatMolName
    , TMP05indexmem = KatOrgName
    , TMP06indexmem = oldPrice
    , TMP07indexmem = NewPrice
    , TMP08indexmem = Pick + KatSoprdOpr + KatSoprdSopr + KatSoprNSopr + KatSoprNRec
    , TMP09indexmem = KatSoprdOpr
    , TMP10indexmem = Pick + KatSoprdOpr(Desc) + KatSoprdSopr(Desc) + KatSoprNSopr(Desc) + KatSoprNRec
    , TMP12indexmem = SpSoprnRec
    , TMP13indexmem = cKatMc + cParty + KatSoprNRec
  );


