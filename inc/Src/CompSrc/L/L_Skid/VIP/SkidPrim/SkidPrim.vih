
#ifdef ComponentVersion
#component "L_Skid"
#end


#doc
Объектный интерфейс описывает методы применимости скидок
#end
ObjInterface SkidPrimObj1;
  #doc
  Инициализация как встроенного интерфейса
  #end
  Procedure InitInt (_pRezim : word);
  //******************************************************************************
  #doc
  Установка формата (0-cgRecordView/1-cgRecordEdit)
  #end
  Procedure FormatInt (_pRezim : word);
  //******************************************************************************
  #doc
  Установка фильтров
  #end
  Procedure SetCurrentFormat;
  //******************************************************************************
  #doc
  Обработчик события смены позиции в интерфейсах-контейнерах (KatSkid)
  #end
  Procedure PosChangeInKatSkid (iNREC : comp);
  //******************************************************************************
  #doc
  Обработчик события закрытия интерфейса-контейнера (KatSkid)
  #end
