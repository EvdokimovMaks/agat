  #doc
  необходима для отслеживания событий "cmDefault/cmEdit/cmInsert/cmDelete"
  TipCm = cgRecordEdit/cgRecordInsert/cgRecordDelete
  #end
  Event Procedure isCmCurRec (TipCm : word);
