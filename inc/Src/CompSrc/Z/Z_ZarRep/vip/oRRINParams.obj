#ifndef _oRRINPARAMS_OBJ_INCLUDED_
#define _oRRINPARAMS_OBJ_INCLUDED_

#Component "Z_ZarRep"

// Описание объектного интерфейса параметров для отчета
objInterface oRRINParams;

  property prRYearOtch    : word       read; // #docl год, за который формируется отчёт
  property prMonth1       : word       read; // #docl месяц начала формирования
  property prMonth2       : word       read; // #docl месяц окончания формирования
  property prMonth3       : word       read; // #docl с какого месяца правильно считается соцналог
  property prVidOtch      : word       read; // #docl вид отчёта
  property prEncoding     : word       read; // #docl вид кодировки
  property prVidRab       : word       read; // #docl вид работы
  property prRazder60001  : word       read; // #docl формирование Дополнительной формы
  property prR2005Mnth    : word       read; // #docl месяц, с кот. расчет соцналога производился с уменьшением на сумму соцотчислений
  property prTune600      : word       read; // #docl замена настройки UP.ZAR.XP.FLAG6
  property prFIO_isp      : string[50] read; // #docl ФИО исполнителя
  property prCodeGni      : string     read; // #docl код ГНИ
  property prNameGni      : string     read; // #docl наименование ГНИ
  property prTypes201     : word       read; // #docl вид расчета(для xml-отчета)
  property prsaverec      : comp       read; // #docl ставка налога (Perech.Nrec)
  property prsummames     : word       read; // #docl сумма строки 600.00.007С предыдущей декларации
  property prSrok         : integer    read; // #docl срок в месяцах превышения депонирования
  property prDataOkNalPer : date       read; // #docl дата окончания отчетного налогового периода
  property prDataOtch     : date       read; // #docl дата отчета
end;
#end