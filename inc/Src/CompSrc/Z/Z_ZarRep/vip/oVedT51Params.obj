#Component "Z_ZarRep"

// Описание объектного интерфейса параметров для отчета "Расчетная ведомость (Типовая форма N Т-51)"
objInterface oVedT51Params;

  property prMonth1 : word read;      // #docl Параметр Начальный месяц периода
  property prMonth2 : word read;      // #docl Параметр Конечный месяц периода
  property prTypeReport : word read;  // #docl Параметр Тип отчета </brief><br> 0 - Расчетная ведомость(от 05.01.2004), <br> 1 - Вкладные листы(от 05.01.2004)
                                      //       <br> 2 - Расчетная ведомость(от 05.01.2004)- формат А3, <br> 3 - Пользовательские отчеты, <br> 4 - Ведомость начислений текущего месяца
  property prUserZPBank : word read;  // #docl Признак отражения суммы удержаний ЗП в банк </brief><br> 0 - в графу 14 (удержано и зачтено), <br> 1 - в графу 18 (к выплате), <br> 2 - согласно входимости
  property prSort : word read;        // #docl Порядок сортировки в отчете </brief><br> 0 - по табельному номеру, <br> 1 - по фамилии
  property prWorkingRejim : word read;// #docl Режим работы фильтра по работникам </brief><br> 0 - обычный режим, <br> 1 - проверка подразделения в табеле, <br> 2 - проверка цеха отнесения затрат

  property prFiltrPodr : string read; // #docl Заданный фильтр по подразделению
  property prNaimK8 : string read;    // #docl Параметр Наименование колонки 8
  property prNaimK9 : string read;    // #docl Параметр Наименование колонки 9
  property prNaimK10 : string read;   // #docl Параметр Наименование колонки 10
  property prNaimK14 : string read;   // #docl Параметр Наименование колонки 14
  property prNaimK12 : string read;   // #docl Параметр Наименование колонки 12
  property prNaimK15 : string read;   // #docl Параметр Наименование колонки 15
  property prNaimK16 : string read;   // #docl Параметр Наименование колонки 16
  property prNaimK18 : string read;   // #docl Параметр Наименование колонки 18
  property prNaimK20 : string read;   // #docl Параметр Наименование колонки 20
  property prNaimK23 : string read;   // #docl Параметр Наименование колонки 23
  property prNaimK24: string read;    // #docl Параметр Наименование колонки 24

  property prDataOtch : date read;    // #docl Отчетная дата
end;

objInterface oVedT51Params_01;
  property prlNumDoc : longint read;  // #docl № документа
end;

objInterface oVedT51Params_02;
  property prNameSignatory : string read; // #docl ФИО составителя документа документа
  property prPostSignatory : string read; // #docl должность составителя документа
end;
