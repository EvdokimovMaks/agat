#Component "Z_ZarRep"

// Описание объектного интерфейса параметров для отчета по оплате труда
objInterface oOplTParams;

  property prVedYear1 : word read;     // #docl Отчетный год за который формируются данные
  property prVedMonth1 : word read;    // #docl Отчетный месяц с которого формируется отчет
  property prVedMonth2 : word read;    // #docl Отчетный месяц по который формируется отчет
  property prPoCexOZ : word read;      // #docl Параметр выгрузки данных </brief><br> 0 - По подразделениям учета,<br> 1 - По подразделениям отнесения затрат
  property prLocFlag6 : word read;     // #docl Параметр учета начислений </brief><br> 0 - По месяцу, в котором начислены,<br> 1 - По месяцу, за который начислены
  property prShowVidOpl : boolean read;   // #docl Параметр Печатать виды оплат в итоговой аналитической ведомости
  property prShowFOT : boolean read;      // #docl Параметр Показывать налоги на ФОТ
  property prCheckNastrKVO : boolean read;// #docl Параметр Учитывать признак "Формировать проводку" вида оплаты
  property prPrintExcel : boolean read;   // #docl Параметр Выгружать в Excel
  property prPrintTXT : boolean read;     // #docl Параметр Выгружать в Бизнес-текст
  property prMonthByNach : boolean read;  // #docl Параметр Отображать месяц по начислению
  property prControlAnaliticVed : boolean read;//#docl Параметр печати отчета Контрольная аналитическая ведомость по оплате труда
  property prTotalAnaliticVed : boolean read;  //#docl Параметр печати отчета Итоговая аналитическая ведомость по оплате труда
  property prUseTXO : boolean read;            //#docl Параметр Формирование ведомости с использованием ТХО
  property prAccountant : string read; // #docl Параметр отчета Бухгалтер
  property prAuthor : string read;     // #docl Параметр отчета Ответственный за выпуск
end;

// Расширение объектного интерфейса параметров для отчета по оплате труда
objInterface oOplTParams_01;

  property prRoundFot : word read;     // #docl Параметр округлений налогов на фот
  property prAccurancy : byte read;    // #docl Параметр количество знаков после запятой
end;
