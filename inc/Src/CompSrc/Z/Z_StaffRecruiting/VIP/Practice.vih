#ifndef _Practice_INCLUDED_
#define _Practice_INCLUDED_

#component "Z_StaffRecruiting"

#doc
Окно просмотра данных о прохождении практики
#end
ObjInterface IPractice;

  #doc
  Процедура установки начальных параметров для формирования логической таблицы
  0 - все доступно для редактирования
  1 - все запрещено
  #end
  procedure SetRelation(Value: comp; TypeVision: word);

  #doc
  Запасная процедура на данный момент
  #end
  procedure SetView(ModView: word; cInfPsn2: comp);

  #doc
  Процедура, сообщающая интерфейсу-владельцу об изменении данных. Если нужно
  #end
  event procedure OnIdle(Sender: comp); // сообщили об этом

  #doc
  Процедура перерисовки доступных панелей
  #end
  procedure ReDrawPanels;

  #doc
  Безусловная (жесткая) перерисовка всех панелей (ReReadRecord)
  #end
  procedure ReReadRecords;

  #doc
  Далее функции Итератора по записям практики. До загрузки обязательно требуется SetRelation
  После выполнения этого доступна нормальная работа GetNextRecord
  #end
  function GetFirstRecord: comp;

  #doc
  работает корректно только после вызова GetFirstRecord
  #end
  function GetNextRecord: comp;

  #doc
  Далее функции Итератора по записям практики. До загрузки обязательно требуется SetRelation
  После выполнения этого доступна нормальная работа GetPrevRecord
  #end
  function GetLastRecord: comp;

  #doc
  работает корректно только после вызова GetLastRecord
  #end
  function GetPrevRecord: comp;

  #doc
  Процедура присвоения переменной sMenu подходящего для данного случая названия меню
  #end
  procedure prSetCorrectMenu (sValue: string);

end;

VipInterface Practice
  implements IPractice
  Licensed(Free);

#end
