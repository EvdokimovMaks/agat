#component "Z_ExpImp"

#ifndef _CloneCreator_vih_included_
#define _CloneCreator_vih_included_

private objinterface ICloneCreator;
  #doc
  Добавить запись в перечень замен</brief>.
  <p>При создании запроса вместо поля с именем <code>_fieldName</code> в запрос для вставки записей будет подставлена строка <code>_subst</code>.</p>
  #end
  procedure AddSubstitution(_fieldName, _subst: string);

  #doc
  Очистить перечень замен</brief>.
  #end
  procedure ClearSubstitutions;

  #doc
  Выполнить запрос на вставку записей</brief>.
  При создании запроса вместо поля с именем <code>_fieldName</code> в запрос для вставки записей будет подставлена строка <code>_subst</code>.</p>
  #end
  function DoQuery(_tableCode: word; _whereCondition: string): integer;
end;

private vipinterface CloneCreator
  implements ICloneCreator
  licensed(free);

#endif
