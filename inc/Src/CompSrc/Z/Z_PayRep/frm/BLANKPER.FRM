.endvar
.function GetfirstPartAdrs(strValue: string; wValue: word): string;
  begin
   if length(strValue) > wValue
   {
     GetfirstPartAdrs:=GetPartStr(strValue, wValue);
     if Trim(GetfirstPartAdrs)=''
       GetfirstPartAdrs:=substr(strValue,1, wValue);
     AddrOtKogo_2:= substr(strValue, length(GetfirstPartAdrs)+1, length(strValue));
   }
   else
   {
     GetfirstPartAdrs:= strValue;
     AddrOtKogo_2:= '';
   }
 end.
