    Маркер по видам удержаний - "Подоходный налог"
  #end
  property mrkUdPodohodNal: TPtr read;
  #doc
    Маркер по видам удержаний - "По исполнительным документам"
  #end
  property mrkUdIspList: TPtr read;
  #doc
    Маркер по видам удержаний - "Прочие"
  #end
  property mrkUdProcie: TPtr read;
end;

VipInterface RPKAZ_T1
  implements iT1Param
