  #doc
    Конечный месяц формирования отчета
  #end
  property monEnd: word read;
  #doc
    Год формирования отчета
  #end
  property wYear: word read;
  #doc
    Порядок сортировки отчета
  #end
  property Sortirovka: word read;
  #doc
    ИНН/БИН
  #end
  property inn_bin: string read;
  #doc
    Маркер по видам оплат - "Повременно"
  #end
  property mrkNachPovrem: TPtr read;
  #doc
    Маркер по видам оплат - "Сдельно"
  #end
  property mrkNachSdelno: TPtr read;
  #doc
    Маркер по видам оплат - "За работу в ночное время"
  #end
  property mrkNachNochVrem: TPtr read;
  #doc
    Маркер по видам оплат - "За работу в праздничные дни"
  #end
  property mrkNachPrazd: TPtr read;
  #doc
    Маркер по видам оплат - "За работу в выходные дни"
  #end
  property mrkNachVihDn: TPtr read;
  #doc
    Маркер по видам оплат - "Доплата за сверх урочное время"
  #end
  property mrkNachSverhUroch: TPtr read;
  #doc
    Маркер по видам оплат - "Премия"
  #end
