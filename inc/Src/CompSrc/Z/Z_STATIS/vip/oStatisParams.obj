#Component "Z_Statis"

// Описание объектного интерфейса параметров статистических данных
objInterface oStatisParams;

  property prLite : word read;           // #docl Признак упрощенного расчет среднесписочной численности
  property prTblCode : word read;        // #docl Код таблицы для дополнительной аналитики
  property prOtladka : word read;        // #docl Печать отладочной информации
  property prLevel : word read;          // #docl Уровень иерархии подразделений
  property prLocFlag6 : word read;       // #docl Параметр учета удержаний </brief><br> 0 - По месяцу, в котором удержаны,<br> 1 - По месяцу, за который удержаны
  property prCalcDog : word read;        // #docl Расчет средней численности по договорам подряда </brief><br> 0 - По межпериоду,<br> 1 - По межпериоду и архиву,<br> 2 - По табелю
  property prFormatReport : word read;   // #docl Формат отчета о расчете среднесписочной численности </brief><br> 0 - Excel, <br> 1 - FastReport

  property prAddSum : boolean read;      // #docl Параметр Добавлять суммы, начисленные в текущем месяце за предыдущие
  property prIncDopAn : boolean read;    // #docl Параметр C учетом подразделения в дополнительной аналитике

  property prFiltrPodr : string read;    // #docl Выбранный фильтр по подразделениям

  property prMon1 : word read;           // #doc1 Пометка Месяцы для пересчета
  property prMon2 : word read;           // #doc1 Пометка Месяцы для пересчета
  property prTypeReport : word read;     // #docl Параметр Формат отчета </brief><br> 0 - XML, <br> 1 - Excel
  property prYear1 : word read;          // #docl Параметр Год для пересчета
  property prDataOtch : date read;       // #docl Параметр Отчетная дата
end;

objInterface oStatisParams_1;
  property prProtocol : boolean read;
end;
