#component "Z_Statis"

#doc
Расчет среднесписочной численности (версия 2)
#end
ObjInterface IStatisObjV2;

  #doc
  Значение коэффициента среднесписочной численности
  #end
  property GetSrChisl: double read;

  #doc
  Значение коэффициента среднесписочной численности по инвалидам
  #end
  property GetSrChislInv: double read;

  #doc
  Режим обработки совместителей
  #end
  property GetLschSovm: word read;

  #doc
  Средняя продолжительность рабочего дня на предприятии
  #end
  property GetAvgHours: double read;

  #doc
  Инициализация даты на которую подсчитывается среднесписочная
  #end
  property SetDate : date write;

  #doc
  Инициализация периода
  #end
  property SetPeriod: longint write;

  #doc
  Расчет среднесписочной
  #end
  procedure RaschSrednSpisochn(cLsch: comp; TypeRasch: byte; CalcDog: word);

  #doc
  Среднесписочная для договорников по межпериоду и архиву
  #end
  function fDogPrvidop( cLsch: comp; dBeg, dEnd: date; wTypeDog: word; var dCount_In: double ): double;
end;

ObjInterface IStatisObjV3;
  #doc
    Инициализация печати протокола расчета среднесписочной численности
  #end
  property SetProtocol : boolean write;
end;

ObjInterface IStatisObjV4;
  #doc
    Печать отладочного протокола расчета среднесписочной численности
  #end
  procedure PrintProtocolSSCH(DateOtch : date);

  #doc
    Очищает временные таблицы
  #end
  procedure ClearTemporyTable;
end;

VipInterface  StatisObj
  implements IStatisObjV2, IStatisObjV3, IStatisObjV4
  licensed (FREE);
