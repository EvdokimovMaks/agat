#declare SetDef_Deb (TableName)
begin
  var I : Integer;
  #TableName.CSCHETD   := cgDefSch;
  #TableName.CSUBSCHD  := cgDefSub;
  for (I := cFirstKAU; I <= cLastKAU; I := I + 1)
  {
    #TableName.TBLD[I]   := 0;
    #TableName.CKAUD[I]  := SetDefKodeKau;
  }
end;
#end //SetDef_Deb

