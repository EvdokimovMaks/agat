#ifndef _GSLoader_obj_INCLUDED_
#define _GSLoader_obj_INCLUDED_

#include GStorage.obj
#include SStorage.obj

#component "Z_Zar"

ObjInterface IGSLoader;
  #doc
    Метод загрузки конфигурации группировок/сортировок в объектные таблицы группировок/сортировок </brief>
    Реализуется последовательностью вызовов методов Add объектов GroupingStorage, SortingStorage с нужными параметрами.
    Реализация этого объектного интерфейса должна быть для каждой конфигурации группировок/сортировок своя.
  #end
  function Load(GroupingStorage: IGStorage; SortingStorage: ISStorage): boolean;

End;

#end
