  #end
  procedure StaffDoWin5;

  event procedure OnStaffDoWin5;

  #doc
  Открытие 6 окна
  #end
  procedure StaffDoWin6;

  event procedure OnStaffDoWin6;

  #doc
  Открытие 7 окна
  #end
  procedure StaffDoWin7;

