table struct tsOtpusk
(
  ID: tNrec,
  cFactOtpusk: tNRec,
  cLsch: tNrec,
  cVacations: tNRec,
  DataN: date,
  DateBShift: date,
  DatOk: date,
  FactChoice: word,
  FilialNo: longint,
  FilialNoKL: longint,
  IsRabDn: word,
  KolDn: double,
  Kotpus: word,
  Modul: word,
  RecFlags: longint,
  NewOriginalNRec: tNrec,
  NOtpus: s200,
  OriginalDataN: date,
  OriginalDatOk: date,
  OriginalKolDn: double,
  OriginalNRec: tNrec,
  Summa: tSumma,
  SumOpl: tSumma
)
