.begin
   fplog('Universal_spec Название = '+Название+', СтНДСНак=' +string(СтНДСНак,0,2) +', КолФ='+string(КолФ,0,2)
     +', спецификацияМЦ_нрек='+string(спецификацияМЦ_нрек,0,0) );
   if wGroup=0
     NeedPrintSpec :=true;
   else
     NeedPrintSpec := (vSpSoprMemTTN.getfirst SpSoprMemTTNGR where (( comp(спецификацияМЦ_нрек) == SpSoprMemTTNGR.Nrec )) =tsok);
   if NeedPrintSpec and wGroup>0//ERP-233
   { fplog('Universal_spec 002  СтНДСНак=' +string(СтНДСНак,0,2));
     if КолФ <> vSpSoprMemTTN.SpSoprMemTTNGR.KolFact
     or СтНДСНак <> if(Валюта_нрек=0, vSpSoprMemTTN.SpSoprMemTTNGR.SumNDS, vSpSoprMemTTN.SpSoprMemTTNGR.SumVNDS)
     {

       СтНДСНак := if(Валюта_нрек=0, vSpSoprMemTTN.SpSoprMemTTNGR.SumNDS, vSpSoprMemTTN.SpSoprMemTTNGR.SumVNDS);
       // пока просто пересчитаем
       if КолФ<>0
       { //СтНакСН   := Round(СтНакСН   * vSpSoprMemTTN.SpSoprMemTTNGR.KolFact/КолФ, 2);
         ЦАкцизНак := Round(ЦАкцизНак * vSpSoprMemTTN.SpSoprMemTTNGR.KolFact/КолФ, 2);
       }
       СтНакСН  := if(Валюта_нрек=0, vSpSoprMemTTN.SpSoprMemTTNGR.RPrice, vSpSoprMemTTN.SpSoprMemTTNGR.RVPrice)
                       * vSpSoprMemTTN.SpSoprMemTTNGR.KolOpl
                      + if(vhodNal<>1,СтНДСНак ,0 )
                       ;
       КолФ := vSpSoprMemTTN.SpSoprMemTTNGR.KolFact;

       fplog('Universal_spec 003  СтНДСНак=' +string(СтНДСНак,0,2)
          +', СтНакСН= '+string(СтНакСН,0,2)
          +', КолФ= '+string(КолФ,0,2)
          +', SpSoprMemTTNGR.KolFact='+string(vSpSoprMemTTN.SpSoprMemTTNGR.KolFact)
          +', KolOpl='                +string(vSpSoprMemTTN.SpSoprMemTTNGR.KolOpl )
          +', SpSoprMemTTNGR.SumNDS=' +string(vSpSoprMemTTN.SpSoprMemTTNGR.SumNDS )
          );

     }
   }
   case wGroup of
   2,3 :
   { if g_ArtClassCode<>0
     { var _str : string;
       _str := GetSegName(g_ArtClassCode,  vSpSoprMemTTN.SpSoprMemTTNGR.cMcUsl);
       if _str<>''
         Название := _str;
     }
   }
   4:// по дополнительному штрих-коду ppGroupAddCode  : Word = 4096;  // по дополнительному штрих-коду
   { if vSpSoprMemTTN.SpSoprMemTTNGR.PrMc=1
       if vSpSoprMemTTN.Getfirst KAtMc=tsok
         if vSpSoprMemTTN.KAtMc.BarKod2 <>''
           Название := vSpSoprMemTTN.KAtMc.BarKod2;
   }
   end;

 End.
