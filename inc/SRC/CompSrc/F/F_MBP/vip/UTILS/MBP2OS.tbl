Table Struct S_MBP2OS
(
  NRec : Comp,
  Name : string,
  Numb : s30,
  KodGr: String[20],
  MatGr: Word,
  isAll: Word
)
With INDEX
(
  S_MBP2OS00 = NRec (Unique),
  S_MBP2OS01 = Name + Numb + NRec (Unique),
  S_MBP2OS02 = Numb + Name + NRec (Unique)
);

