  function IsValidPeriod(strIn: string; var strGetMes: string) : boolean;
  begin
    //Налоговый период
    //Период
    strGetMes := 'Возможные значения: 34, 90, 94';

    Result := False;
    Result :=
    (
         (strIn = '34') or (strIn = '90') or (strIn = '94')
    );
  end;

  function IsValidOKEI_Local(strIn: string; var strGetMes: string) : boolean;
  begin
    //Код из Общероссийского классификатора единиц измерения
    //ОКЕИ
    strGetMes := 'Возможные значения: 384';

    Result := False;
    Result :=
    (
         (strIn = '384')
    );
  end;

  #declare _NoKor(_AdrXML, _Pole)
    MessageErrorStop_ALG
    (
      #_AdrXML,
      NameStr,
      #_Pole,
      'Поле не соответствует формату. ' + getMes
    );
  #end

  procedure ValidPoyAndSum(adrPoly, NS, NStr: string; flEx1, flEx2, flEx3, flEx4: boolean);
  begin
    if GetFldVis(adrPoly)
    {
      var NameStr, getMes: string;
      NameStr:=''; getMes:='';
      NameStr := NS;

      if flEx1 {if not IsValidLenStr(GetStrVal(adrPoly+'/Пояснения'), 1, 4, getMes) #_NoKor(adrPoly+'/Пояснения', NStr+', гр. 1');}
      else     {if GetFldVis(adrPoly+'/Пояснения') if not IsValidLenStr(GetStrVal(adrPoly+'/Пояснения'), 1, 4, getMes) #_NoKor(adrPoly+'/Пояснения', NStr+', гр. 1');}

      if flEx2 {if not IsValidLenNumber(GetStrVal(adrPoly+'/СумОтч'), 1, 12, getMes, True) #_NoKor(adrPoly+'/СумОтч', NStr+', гр. 4');}
      else     {if GetFldVis(adrPoly+'/СумОтч') if not IsValidLenNumber(GetStrVal(adrPoly+'/СумОтч'), 1, 12, getMes, True) #_NoKor(adrPoly+'/СумОтч', NStr+', гр. 4');}

      if flEx3 {if not IsValidLenNumber(GetStrVal(adrPoly+'/СумПрдщ'), 1, 12, getMes, True) #_NoKor(adrPoly+'/СумПрдщ', NStr+', гр. 5');}
      else     {if GetFldVis(adrPoly+'/СумПрдщ') if not IsValidLenNumber(GetStrVal(adrPoly+'/СумПрдщ'), 1, 12, getMes, True) #_NoKor(adrPoly+'/СумПрдщ', NStr+', гр. 5');}

      if flEx4 {if not IsValidLenNumber(GetStrVal(adrPoly+'/СумПрдшв'), 1, 12, getMes, True) #_NoKor(adrPoly+'/СумПрдшв', NStr+', гр. 6');}
      else     {if GetFldVis(adrPoly+'/СумПрдшв') if not IsValidLenNumber(GetStrVal(adrPoly+'/СумПрдшв'), 1, 12, getMes, True) #_NoKor(adrPoly+'/СумПрдшв', NStr+', гр. 6');}
    }
  end;

  procedure ValidVPokOPPTip(adrPoly, NS, NStr: string);
  begin
    var NameStr, getMes: string;
    var i: integer;
    NameStr:=''; getMes:='';
    i:=0;

    for (i := 0; i < XMLMap.GetNodeCountByName(adrPoly); i++)
    {
      if GetFldVis(adrPoly+GetIter(i))
      {
        NameStr := NS;
                                                      if not IsValidLenStr   (GetStrVal(adrPoly+GetIter(i)+'/НаимПок'  ), 1, 255, getMes      ) #_NoKor(adrPoly+GetIter(i)+'/НаимПок'  , NStr+String(i+1)+', гр. 1');
        if GetFldVis(adrPoly+GetIter(i)+'/Пояснения') if not IsValidLenStr   (GetStrVal(adrPoly+GetIter(i)+'/Пояснения'), 1, 4  , getMes      ) #_NoKor(adrPoly+GetIter(i)+'/Пояснения', NStr+String(i+1)+', гр. 2');
                                                      if not IsValidLenNumber(GetStrVal(adrPoly+GetIter(i)+'/СумОтч'   ), 1, 12 , getMes, True) #_NoKor(adrPoly+GetIter(i)+'/СумОтч'   , NStr+String(i+1)+', гр. 4');
        if GetFldVis(adrPoly+GetIter(i)+'/СумПрдщ'  ) if not IsValidLenNumber(GetStrVal(adrPoly+GetIter(i)+'/СумПрдщ'  ), 1, 12 , getMes, True) #_NoKor(adrPoly+GetIter(i)+'/СумПрдщ'  , NStr+String(i+1)+', гр. 5');
        if GetFldVis(adrPoly+GetIter(i)+'/СумПрдшв' ) if not IsValidLenNumber(GetStrVal(adrPoly+GetIter(i)+'/СумПрдшв' ), 1, 12 , getMes, True) #_NoKor(adrPoly+GetIter(i)+'/СумПрдшв' , NStr+String(i+1)+', гр. 6');
      }
    }
  end;

  procedure ValidOPP_VPTip(adrPoly, MnElem, NS1, NS2, NStr: string);
  begin
    if GetFldVis(adrPoly)
    {
      ValidPoyAndSum (adrPoly           , NS1, NStr, False, True, False, False);
      ValidVPokOPPTip(adrPoly+'/'+MnElem, NS2, NStr                           );
    }
  end;

