  procedure VPokOPPTip(strAdr: string);
  begin
    var i : integer; i:=0;
    for (i := 0; i < XMLMap.GetNodeCountByName(strAdr); i++)
    {
      if not GetStrNull(strAdr+GetIter(i)+'/НаимПок') and not GetStrNull(strAdr+GetIter(i)+'/СумОтч')
      {
        #SetVisT(strAdr+GetIter(i)             );
        #IfVis  (strAdr+GetIter(i)+'/Пояснения');
        #IfVis  (strAdr+GetIter(i)+'/СумПрдщ'  );
        #IfVis  (strAdr+GetIter(i)+'/СумПрдшв' );
      }
      else #SetVisF(strAdr+GetIter(i));
    }
  end;

  procedure OPP_VPTip(strAdr: string);
