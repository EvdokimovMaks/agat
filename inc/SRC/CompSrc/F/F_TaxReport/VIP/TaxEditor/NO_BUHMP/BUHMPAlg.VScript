@Script On_Visable : boolean;
  #declare IfVis(adrIfVis)
    SetFldVis(#adrIfVis, not GetStrNull(#adrIfVis));
  #end
  #declare SetVisF(adrIfVis)
    SetFldVis(#adrIfVis, False);
  #end
  #declare SetVisT(adrIfVis)
    SetFldVis(#adrIfVis, True);
  #end

  procedure OPP_VPTip(strAdr: string);
  begin
    if     not GetStrNull(strAdr+'/КодСтрок'   )
       and not GetStrNull(strAdr+'/Сум31.12Отч')
    {
      #SetVisT  (strAdr             );
      #IfVis    (strAdr+'/Пояснения');
      #IfVis    (strAdr+'/Сум31.12Прдщ'  );
      #IfVis    (strAdr+'/Сум31.12Прдшв' );
    }
    else #SetVisF(strAdr);
  end;

  function ValidationOKOPF : boolean;
  begin
    //ПрибУб
    //Элемент обязателен, за исключением случая, когда <ОКОПФ> принимает следующие значения:
    //  в соответствии с ОКОПФ (ОК 028-99) 85
    //  в соответствии с ОКОПФ (ОК 028-2012) 20101 | 20102 | 20103 | 20104 | 20105 | 20106 | 20107 | 20109 | 20110 | 20111 | 20112 | 20113 | 20114 | 20115 | 20202 |
    var ValueOKOPF : string; ValueOKOPF:='';
    ValueOKOPF := GetStrVal('Файл/Документ/СвНП/ОКОПФ');

    Result :=
      ValueOKOPF='85' or
      ValueOKOPF='20101' or ValueOKOPF='20102' or ValueOKOPF='20103' or ValueOKOPF='20104' or ValueOKOPF='20105' or ValueOKOPF='20106' or ValueOKOPF='20107' or
      ValueOKOPF='20109' or ValueOKOPF='20110' or ValueOKOPF='20111' or ValueOKOPF='20112' or ValueOKOPF='20113' or ValueOKOPF='20114' or ValueOKOPF='20115' or
      ValueOKOPF='20202';
  end;

  procedure OtchPredNDopTip(strAdr: string);
  begin
    if     not GetStrNull(strAdr+'/КодСтрок')
       and not GetStrNull(strAdr+'/СумОтч'  )
    {
