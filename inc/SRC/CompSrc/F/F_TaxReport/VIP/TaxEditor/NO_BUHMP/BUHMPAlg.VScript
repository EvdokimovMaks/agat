  function DvigKapPGodAll(strAdr: string; fl1, fl2, fl3, fl4, fl5, fl6, flS: boolean) : boolean;
  begin
    if fl1 and fl6
    {
      if not GetStrNull(strAdr+'/УстКапитал') and not GetStrNull(strAdr+'/Итог')
      {
        if flS {#SetVisT(strAdr); Result := True;}

        if fl2 #IfVis(strAdr+'/СобВыкупАкц');
        if fl3 #IfVis(strAdr+'/ДобКапитал' );
        if fl4 #IfVis(strAdr+'/РезКапитал' );
        if fl5 #IfVis(strAdr+'/НераспПриб' );
      }
      else if flS {#SetVisF(strAdr); Result := False;}
    }
    else
      if fl1
      {
        if not GetStrNull(strAdr+'/УстКапитал')
        {
          if flS {#SetVisT(strAdr); Result := True;}

          if fl2 #IfVis(strAdr+'/СобВыкупАкц');
          if fl3 #IfVis(strAdr+'/ДобКапитал' );
          if fl4 #IfVis(strAdr+'/РезКапитал' );
          if fl5 #IfVis(strAdr+'/НераспПриб' );
        }
        else if flS {#SetVisF(strAdr); Result := False;}
      }
      else
        if fl6
        {
          if not GetStrNull(strAdr+'/Итог')
          {
            if flS {#SetVisT(strAdr); Result := True;}

            if fl2 #IfVis(strAdr+'/СобВыкупАкц');
            if fl3 #IfVis(strAdr+'/ДобКапитал' );
            if fl4 #IfVis(strAdr+'/РезКапитал' );
            if fl5 #IfVis(strAdr+'/НераспПриб' );
          }
          else if flS {#SetVisF(strAdr); Result := False;}
        }
        else
        {
          if fl2 #IfVis(strAdr+'/СобВыкупАкц');
          if fl3 #IfVis(strAdr+'/ДобКапитал' );
          if fl4 #IfVis(strAdr+'/РезКапитал' );
          if fl5 #IfVis(strAdr+'/НераспПриб' );

          if    (fl2 and GetFldVis(strAdr+'/СобВыкупАкц'))
             or (fl3 and GetFldVis(strAdr+'/ДобКапитал' ))
             or (fl4 and GetFldVis(strAdr+'/РезКапитал' ))
             or (fl5 and GetFldVis(strAdr+'/НераспПриб' ))
          then #SetVisT(strAdr)
          else #SetVisF(strAdr);
        }
  end;

