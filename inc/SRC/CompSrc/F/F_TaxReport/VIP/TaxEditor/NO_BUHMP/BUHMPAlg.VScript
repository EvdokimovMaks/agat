  procedure ValidPoyAndSum(adrPoly, NS, NStr: string; flEx1, flEx2, flEx3, flEx4, flEx5: boolean);
  begin
    if GetFldVis(adrPoly)
    {
      var NameStr, getMes: string;
      NameStr:=''; getMes:='';
      NameStr := NS;

      if flEx1 {if not IsValidLenStr(GetStrVal(adrPoly+'/Пояснения'), 1, 3, getMes) #_NoKor(adrPoly+'/Пояснения', NStr+', гр. 1');}
      else     {if GetFldVis(adrPoly+'/Пояснения') if not IsValidLenStr(GetStrVal(adrPoly+'/Пояснения'), 1, 3, getMes) #_NoKor(adrPoly+'/Пояснения', NStr+', гр. 1');}

      if flEx2 {if not IsValidLenNumber(GetStrVal(adrPoly+'/КодСтрок'), 4, 4, getMes, False) #_NoKor(adrPoly+'/КодСтрок', NStr+', гр. 3');}
      else     {if GetFldVis(adrPoly+'/КодСтрок') if not IsValidLenNumber(GetStrVal(adrPoly+'КодСтрок'), 4, 4, getMes, True) #_NoKor(adrPoly+'/КодСтрок', NStr+', гр. 3');}

      if flEx3 {if not IsValidLenNumber(GetStrVal(adrPoly+'/Сум31.12Отч'), 1, 12, getMes, True) #_NoKor(adrPoly+'/Сум31.12Отч', NStr+', гр. 4');}
      else     {if GetFldVis(adrPoly+'/Сум31.12Отч') if not IsValidLenNumber(GetStrVal(adrPoly+'/Сум31.12Отч'), 1, 12, getMes, True) #_NoKor(adrPoly+'/Сум31.12Отч', NStr+', гр. 4');}

      if flEx4 {if not IsValidLenNumber(GetStrVal(adrPoly+'/Сум31.12Прдщ'), 1, 12, getMes, True) #_NoKor(adrPoly+'/Сум31.12Прдщ', NStr+', гр. 5');}
