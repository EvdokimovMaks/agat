@Script Prov_Length(mesPri_t: comp; Adr, plStr, plPole, plMes: string; Min, Max: integer; flag: boolean): boolean;
@begin
  var bufStr: string;
  Result := False;

  bufStr := GetStrVal(Adr);
  if ((flag) or ((not flag) and (bufStr <> '')))
  {
    if (Max = -1)
    {
      if (Length(bufStr) <> Min)
        if (MessageErrorStop(Adr, mesPri_t, plStr, plPole, plMes)) Exit;
    }
    else
    {
      if (Min = -1)
      {
        if (Length(bufStr) > Max)
          if (MessageErrorStop(Adr, mesPri_t, plStr, plPole, plMes)) Exit;
      }
      else
      {
        if ((Length(bufStr) < Min) or (Length(bufStr) > Max))
          if (MessageErrorStop(Adr, mesPri_t, plStr, plPole, plMes)) Exit;
      }
    }
  }
@end.

