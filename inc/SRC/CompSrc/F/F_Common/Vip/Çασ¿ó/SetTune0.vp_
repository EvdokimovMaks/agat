procedure SetNewPlansNo;
var
  isOk : boolean;
{
  ReSetBounds(#PlansSch);
  isOk := (GetFirst PlansSch where ((myPlansNo == PlansSch.NRec)) = tsOk);
  SetBounds(#PlansSch);
  if (not isOk) Exit;
  coSetTune('Fin.User.cPlansSch',myPlansNo,PlansSch.Name);
}
