Table struct TblCubeMeasure
(
  Npp      : word
, isShow   : boolean
, Title    : string
)
with index
(
  TblCubeMeasure01 = Npp (Unique),
  TblCubeMeasure02 = isShow + Npp
);

Table struct TblPrnSpAxis
(                   
