, Npp       : word
, Ind       : LongInt
, isLeaf    : word
, Level     : word
, sAbbr     : s20
, sEd       : s20
, sVal      : s20
, Name      : s80
)
with index
(
  TblPrnSpAxis01 = XY + Npp
);

