.function GetPeriodName(_period, _year: word): string;
begin
  case _period of
    1: Result := 'С 01.01.' + _year + ' ПО 31.03.' + _year;
    2: Result := 'С 01.04.' + _year + ' ПО 30.06.' + _year;
    3: Result := 'С 01.07.' + _year + ' ПО 30.09.' + _year;
    4: Result := 'С 01.10.' + _year + ' ПО 31.12.' + _year;
   end;
end.
