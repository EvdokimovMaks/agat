.begin
  НомерПачкиОсн :=  PachNumBegin;
  НомерВпачке   :=  1;
  ТипКавычек    := if( wQuotes = 0, '"', '&quot;' );
  НПред         :=  Replace( Upcase( Trim( НаимПред ) ), '"', ТипКавычек);
  НПредКраткое  :=  Replace( Upcase( Trim( НаимПредКраткое ) ), '"', ТипКавычек);
  Форма         :=  Upcase( Trim( формасобств) );
!
  if ( wRound = 2 )
    sRound := '[|-]3666666666666667.88';
  else
    sRound :='[|-]3666666666666667';
!
  case ВидФормы of
    0: ТипСведений := 'ИСХОДНАЯ';
    1: ТипСведений := 'КОРРЕКТИРУЮЩАЯ';
    2: ТипСведений := 'ОТМЕНЯЮЩАЯ';
  end;
!--------------------
  if ВидФормы = 0  //исходные сведения
  {
    название_отчетного_периода := GetPeriodName(Период, Год);
    год_отчетного_периода      := Год;
    отчетный_период            := Период;
  }
  else
  {
    название_отчетного_периода       := GetPeriodName(период_корректировки, год_при_корректировке);
    год_отчетного_периода            := год_при_корректировке;
    отчетный_период                  := период_корректировки;
    название_корректируемого_периода := GetPeriodName(Период, Год);
    год_корректируемого_периода      := Год;
    корректируемый_период            := Период
  }
!--------------------
  case Период of
    1: Месяц := 1; // 1 квартал
    2: Месяц := 4; // 1 полугодие
    3: Месяц := 7; // 9 месяцев
    4: Месяц := 10;// год
  end;
!--------------------
end.
