#ifndef _OtpDopStore_vih_INCLUDED_
#define _OtpDopStore_vih_INCLUDED_

#ifdef ComponentVersion
  #component "Z_PAYREP"
#end

objinterface IOtpDopStore;
  #doc
    Позиционируется на запись по ссылке
  #end
  function Seek(Ref: comp): boolean;

  #doc
    Удаление записей в хранилище
  #end
  procedure Clear;

  #doc
    Переходит на первую запись
  #end
  function First: boolean;

  #doc
    Переходит на следующую запись
  #end
  function Next: boolean;

  #doc
    Вставляет запись с данными и возвращает NREC вставленной записи
  #end
  function InsData(B: TYPE$OTPDOP): comp;

  #doc
    Возвращает данные
  #end
  procedure GetData(var B: TYPE$OTPDOP);

  #doc
    Удаляет запись по ссылке
  #end
  procedure DelData(Ref: comp);

end;

vipinterface OtpDopStore implements IOtpDopStore licensed(free);

#end
