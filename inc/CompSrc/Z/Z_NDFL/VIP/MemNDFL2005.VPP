  VidUd183  : word,
  VidUd184  : word,
  VidUd110  : word
)
with index
(
  tpPlPor01 = cPlPor,
  tpPlPor02 = Okato + Kpp + bMes + VidUd110,
  tpPlPor03 = DebSchet + DebSubSch,
  tpPlPor04 = Okato + Kpp + VidUd110,
  tpPlPor05 = Okato + Kpp + bMes + VidUd184
);
