#ifndef _ISpecialAlgorithmIterator_obj_INCLUDED_
#define _ISpecialAlgorithmIterator_obj_INCLUDED_
#ifdef ComponentVersion
  #component "z_calc"
#end

#doc
  Объектный интерфейс итератора по списку спецалгоритмов
#end
objinterface ISpecialAlgorithmIterator;
  #doc
    Устанавливает указатель на первый элемент списка и возвращает true в случае
    успешного завершения.
  #end
  function First: boolean;

  #doc
    Устанавливает указатель на следующий элемент списка и возвращает true в
    случае успешного завершения.
  #end
  function Next: boolean;

  #doc
    Возвращает наименование текущего спецалгоритма.
  #end
  function GetName: string;

  #doc
    Возвращает описание текущего спецалгоритма.
  #end
  function GetDescription: string;
end;

#end
