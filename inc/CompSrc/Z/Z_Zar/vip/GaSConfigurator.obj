#ifndef _GaSConfigurator_obj_INCLUDED_
#define _GaSConfigurator_obj_INCLUDED_

#component "Z_Zar"

ObjInterface IGaSConfigurator;
    
  #doc
  Вызывает загрузчик конфигурации с именем реализации, который следует указать в параметре  </brief>
  Важно:
    При вызове из другого компонента указывать полное имя реализации загрузчика:
    MyGSConfigurator.Configure('Z_Zar::GSLoader_fvedom2_common');
  #end
  procedure Configure(GSLoaderName: string);
  
End;

#end
