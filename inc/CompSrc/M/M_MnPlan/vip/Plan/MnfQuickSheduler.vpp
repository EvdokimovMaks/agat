Table Struct dsReserv
(
 cSpGraf      : tNRec,
 NomerSm      : Word,
 tStart       : Time,
 tEnd         : Time,
 wWorkCentre  : Word,    //'производственный центр (ПЦ)
 cWorkCentre  : tNRec,   //'-//-
 wObject      : Word,    //'объект резерва (то что мы размещаем)
 cObject      : tNRec,   //'-//-
 IsStable     : Boolean  //'твердый/нетвердый резерв (для чтения вне моего объекта
                         //'всегда = True)
)
With Index
(
 dsReserv_01 = wWorkCentre + cWorkCentre + cSpGraf + NomerSm + tStart,
 dsReserv_02 = wObject + cObject + wWorkCentre + cWorkCentre,
 dsReserv_03 = IsStable
);
