Table Struct SObjType (
                      wTypeObj: word, // 1 - программа
                                      // 2 - план
                                      // 4 - потребность в материалах
                                      // 8 - потребность в тр. ресурсах
                                      // 16384 - спецификация программы/плана
                                      // 32768 - НЗП
                      wKindObj: word, // 1 - основание расчета (вх. документы)/ и прочие
                                      // 2 - результаты расчета (исх. документы)
                                      // 4 - нормы и нормативы
                      cObjType: word, // ссылка на wCode
                      wCode: word,    // см. \vip\mnplan\constpl.vpp
                      Name: string[100],
                      Abbr: string[20],
                      wIndex: word
                      )
with Index
                      (
                      SObjType01 = wTypeObj + wKindObj + cObjType + wCode (Unique),
                      SObjType02 = wTypeObj + wKindObj + cObjType + wIndex
                      )
;
