table struct tmpSpmnp
(
  nrec    : comp,   //nrec SPMNPLAN
  mcname  : string, //МЦ
  edname  : string, //Отпускные еденицы
  attr1   : string, //PID
  attr2   : string, //Лот
  attr3   : string, //Ответственное лицо
 // attr4   : string, //Поставка
  enddate : date  , //дата поставки
  fil     : string, //Филиал
  co      : string, //Центр ответственности
  stbud   : string, //Статья Бюджета
  prog    : string, //Программа
  kol     : double,  //Кол-во
  kolZ    : double,  //Кол-во  к заказу
  kolK    : double  //Кол-во   законтрактовано
)
with index
(
  idx1 = nrec,
  idx2 = mcname,
  idx3 = attr1,
  idx4 = attr2,
  idx5 = attr3,
 // idx6 = attr4,
  idx7 = attr1+attr2,
  idx8 = attr1+attr3,
  idx9 = attr1/*+attr4*/,
  idx10= attr1+attr2+attr3,
  idx11= attr1+attr2,
  ///*+attr4*/,
  idx12= attr1+attr3,
  ///*+attr4*/,
  idx13= attr1+attr2+attr3,
  ///*+attr4*/,
  idx14= attr2+attr3,
//  idx15= attr2/*+attr4*/,
  //idx16= attr2+attr3/*+attr4*/,
 // idx17= attr3/*+attr4*/,
  idx18= prog,
  idx19= attr1+prog,
  idx20= attr2+prog,
  idx21= attr3+prog,
//  idx22= attr4+prog,
  idx23= attr1+attr2+prog,
  idx24= attr1+attr3+prog,
  idx25= attr1/*+attr4*/+prog,
  idx26= attr1+attr2+attr3+prog,
  idx27= attr1+attr2/*+attr4*/+prog,
  idx28= attr1+attr3/*+attr4*/+prog,
  idx29= attr1+attr2+attr3/*+attr4*/+prog,
  idx30= attr2+attr3+prog,
  idx31= attr2/*+attr4*/+prog,
  idx32= attr2+attr3/*+attr4*/+prog,
  idx33= attr3/*+attr4*/+prog

);
