table struct  tpSOborot
(   cPlansSch  : comp,
    SchetO     : tSchet3,        //"Основной счет", @
    SubOsSch   : tSubSch,        //"Субсчет основного счета",    @
    TBLOS      : tTabKau,        //"таблицы КАУ основного счета", @
    KAUOS      : tGetKau,        //"КАУ основного счета",         @
    TBLKS      : tTabKau,        //"таблицы КАУ корреспонд.счета",
    KAUKS      : tGetKau,        //"КАУ корреспонд.счета",
    wt_Source  : word,
    DateT      : Date,           //"Дата исход.сальдо",
    NAnn       : word,
    ViOb       : Word,           //"Вид сальдо (д=1,к=2)",
    SchetK     : string[20],     // "Корреспондирующий счет",
    SubSchK    : string[20],     // "Субсчет корреспондирующего счета",
    TotSumma   : Double
)
with Index
(
 tpSOborotInd01 = cPlansSch + ViOb + SchetO + SubOsSch + SchetK + SubSchK + TBLOS[1] + TBLOS[2] +
                  KAUOS[1] + KAUOS[2] + TBLKS[1] + TBLKS[2] + KAUKS[1] + wt_Source,
 tpSOborotInd02 = ViOb + SchetO + SubOsSch + SchetK + SubSchK + TBLOS[1] + TBLOS[2] + KAUOS[1] +
                  KAUOS[2] + TBLKS[1] + TBLKS[2] + KAUKS[1] + wt_Source,
 tpSOborotInd03 = ViOb + SchetO + SubOsSch + SchetK + SubSchK +
                  KAUOS[1] + KAUOS[2] + KAUOS[3] + KAUKS[1] + KAUKS[2] + KAUKS[3]

);
table struct  tMessageT
(
 cMes  : comp
 );







