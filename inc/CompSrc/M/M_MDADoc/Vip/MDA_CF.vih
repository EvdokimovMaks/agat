// Структура для задания фильтра по позициям документа
#ifndef __Def_TMDACellFilter__
#define __Def_TMDACellFilter__
#doc
 Структура для задания фильтра по позициям документа</brief> <br>
 Поля: <br>
   rdDateInterval - Структура для задания интервала дат <br>
   oMDACellAn_BF  - Фильтр по аналитикам <br>
   wFlFindDates   - Флаг поиска дат для позиции: <br>
     0: MDACell -> MDACellAn -> MDADoc -> MDADocAn <br>
     1: MDACellAn -> MDACell -> MDADocAn -> MDADoc <br>
#end
Type TMDACellFilter = record
  rdDateInterval: TDateInterval; // Структура для задания интервала дат
  oMDACellAn_BF: oValuesAn_BF;   // Фильтр по аналитикам
  wFlFindDates: word;            // Флаг поиска дат для позиции:
                                 //   0: MDACell -> MDACellAn -> MDADoc -> MDADocAn
                                 //   1: MDACellAn -> MDACell -> MDADocAn -> MDADoc
End; // Type TMDACellFilter = record
#endif

//------------------------------------------------------------------------------
// Структура для задания параметров при копировании документов
#ifndef __Def_TParamsCopyMDADoc__
#define __Def_TParamsCopyMDADoc__
#doc
 Структура для задания параметров при копировании документов</brief> <br>
 Поля: <br>
   bCopyHead       - Флаг "Копировать шапку документа" <br>
   bCopyDocAn      - Флаг "Копировать аналитики шапки документа" <br>
   bCopyTbls       - Флаг "Копировать представления-фильтры документа" <br>
   bCopyCells      - Флаг "Копировать позиции документа" <br>
   bCopyParams     - Флаг "Копировать параметры документа" <br>
   wOptions        - Флаг или группа флагов: <br>
     1: При копировании параметров учитывать только необходимые параметры для объектов планирования приемника <br>
   oListRltAn      - Список связей между аналитиками <br>
   oListRltLayers  - Список связей между шаблонами значений <br>
   rdMDACellFilter - Фильтр на позиции документа-источника <br>
   wModify         - Флаг модификации существующих значений в позициях документа-приемника <br>
     0: Оставлять существующие значения <br>
     1: Заменять существующие значения новыми <br>
     2: Добавлять новые значения к существующим <br>
     3: Перед расчетом удалять все существующие значения <br>
   iDataShift      - Сдвиг данных во времени (количество периодов) <br>
   wDataShiftUnit  - Единица смещения (из TPPConst.inc) <br>
     xDay     : День <br>
     xWeek    : Неделя <br>
     xTenDay  : Декада <br>
     xMonth   : Месяц <br>
     xQuarter : Квартал <br>
     xHalfYear: Полугодие <br>
     xYear    : Год <br>
   bShowMsg       - Признак выдачи сообщений <br>
#end
Type TParamsCopyMDADoc = record
  bCopyHead: boolean;              // Флаг "Копировать шапку документа"
  bCopyDocAn: boolean;             // Флаг "Копировать аналитики шапки документа"
  bCopyTbls: boolean;              // Флаг "Копировать представления-фильтры документа"
  bCopyCells: boolean;             // Флаг "Копировать позиции документа"
  bCopyParams: boolean;            // Флаг "Копировать параметры документа"
  wOptions: word;                  // Флаг или группа флагов:
                                   //   1: При копировании параметров учитывать только необходимые параметры для объектов планирования приемника
  oListRltAn: oiListRltAn;         // Список связей между аналитиками
  oListRltLayers: oiListRltVal;    // Список связей между шаблонами значений
  rdMDACellFilter: TMDACellFilter; // Фильтр на позиции документа-источника
  wModify: word;                   // Флаг модификации существующих значений в позициях документа-приемника
                                   //   0: Оставлять существующие значения
                                   //   1: Заменять существующие значения новыми
                                   //   2: Добавлять новые значения к существующим
                                   //   3: Перед расчетом удалять все существующие значения
  iDataShift: integer;             // Сдвиг данных во времени (количество периодов)
  wDataShiftUnit: word;            // Единица смещения (из TPPConst.inc)
                                   //  xDay     : День
                                   //  xWeek    : Неделя
                                   //  xTenDay  : Декада
                                   //  xMonth   : Месяц
                                   //  xQuarter : Квартал
                                   //  xHalfYear: Полугодие
                                   //  xYear    : Год
  bShowMsg: boolean;               // Признак выдачи сообщений
End; // Type TParamsCopyMDADoc = record
#endif

