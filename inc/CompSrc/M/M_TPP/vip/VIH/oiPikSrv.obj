#ifndef __OIPIKSRV_VIH
#define __OIPIKSRV_VIH

#ifdef ComponentVersion
#component "M_TPP"
#end

#doc
объектный интерфейс сервис работы с таблицей Pick</brief>
#end

ObjInterface oiPikSrv;

#doc
удалить в таблице <I>Pick</I> все записи с код списка <I>wList</I></brief>
#end
  function ClearAll(wList: integer): integer;

#doc
добавить/удалить запись в таблице <I>Pick</I></brief>
#end
  function InvertItem(wList: integer; cRec: comp): integer;

#doc
добавить/удалить запись в таблице <I>Pick</I> в зависимости от режима работы <I>wMode</I></brief>
#end
  function InOutItem(wList: integer; cRec: comp; wMode: boolean): integer;

#doc
проверка есть ли в таблице <I>Pick</I> запись с такими параметрами</brief>
#end
  function ItemPicked(wList: integer; cRec: comp): integer;

#doc
достать и удалить из Pick</brief>
#end
  function GetItem(wList: integer): comp; 

#doc
проверка нет ли в таблице <I>Pick</I> записи с кодом списка <I>wList</I>.</brief>
В случае если нет таких записей, то функция возвращает <I>true</I>
#end
  function isEmpty(wList: integer): boolean;
end; // oiPikSrv

#endif  // __OIPIKSRV_VIH