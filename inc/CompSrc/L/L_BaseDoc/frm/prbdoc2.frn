#end
.linkform 'PRBDOC_208' prototype is 'PRBDOC'
.NameInList 'Паспорт импортной сделки для России'
.group 'Закупка'
.group 'Предоплата закупок'
.Group 'Country' subGroup 'Russia'
.create view loBD
var
  cBaseDoc : comp;
  oHistory : iOldHistory;
from
  BaseDoc
, KlVal
, KatOrg   Pol
, KatOrg   Post
, KatCity  CityPol
, KatCity  CityPost
, KatState StatePost
, KatBank  BankPost
, KatBank  BankPol
, KlVal    SpVal
where
((
              cBaseDoc == BaseDoc.NRec
and       BaseDoc.cVal == KlVal.NRec
and       BaseDoc.cOrg == Post.NRec
and coGetTune('MyOrg') == Pol.NRec
and         Post.cCity == CityPost.NRec
and          Pol.cCity == CityPol.NRec
and        Post.cState == StatePost.NRec
and      BaseDoc.cBank == BankPost.NRec
and    BaseDoc.cMyBank == BankPol.NRec
and       BaseDoc.NRec == StepDoc.cBaseDoc
and       StepDoc.NRec == SpStep.cStepDoc
and        SpStep.cVal == SpVal.NRec
))
;
.var
  PSINum  : string
  PSIDate : date
  FormRasc: string
  OplTov  : string
.endvar
.begin
  loBD.cBaseDoc := BaseDocNRec;

  if (loBD.GetFirst Pol = 0) {}

  if (loBD.GetFirst BaseDoc = 0)
  {
    var ExtAttr  : IExtAttr;
    var ExtClass : IExtClass;
    var cAttrNam : comp;
    var wTypeAttr: word;

    // ПС
    PSINum    := sGetAttr(coBaseDoc, BaseDocNRec, 'Паспорт сделки (ПС)');

    // дата регистрации ПС
    PSIDate   := dGetAttr(coBaseDoc, BaseDocNRec, 'Дата регистрации ПС');

    //форма расчетов
    wTypeAttr := GetClassCode(coBaseDoc, 'Форма расчетов');

    if (wTypeAttr <> 0)
      FormRasc := ''
    else
    {
      FormRasc := GetSegName(wTypeAttr, BaseDocNRec);

      if (Length(FormRasc) > 2)
        FormRasc := SubStr(FormRasc, 1, 2);
    }

    //оплата товара
    wTypeAttr := GetClassCode(coBaseDoc, 'Оплата товара');

    if (wTypeAttr <> 0)
      OplTov := ''
    else
    {
      OplTov := GetSegName(wTypeAttr,BaseDocNRec);

      if (Length(OplTov) > 2)
        OplTov := SubStr(OplTov, 1, 2);
    }

  };
end.
.fields
  PSINum
  PSIDate

  loBD.BankPol.Name
  loBD.BankPol.Addr

  oHistory.sGetField(coKatOrg, loBD.Pol.nRec, 'NAME', loBD.BaseDoc.dDoc)
  GetKatOrgAttrName4Rep('OKPO', loBD.Pol.nRec)
  loBD.Pol.OKPO loBD.Pol.Addr+', '+loBD.CityPol.Name
  loBD.BankPol.Schet1
  GetKatOrgAttrName4Rep('UNN', loBD.Pol.nRec)
  loBD.Pol.UNN

  oHistory.sGetField(coKatOrg, loBD.Post.nRec, 'NAME', loBD.BaseDoc.dDoc)
  loBD.StatePost.Name
  loBD.StatePost.Kod
  loBD.Post.Addr+', '+loBD.CityPost.Name

  loBD.BaseDoc.NoDoc
  DateToStr(Add_Day(loBD.BaseDoc.ddoc, loBD.BaseDoc.KolDn), 'DD/MM/YYYY')

  loBD.BaseDoc.dDoc
  loBD.KlVal.Dollar
  loBD.BaseDoc.Summa
  loBD.KlVal.ISO

  loBD.SpVal.Dollar
  FormRasc
  loBD.SpVal.ISO
  OplTov
  ''

  '' '' '' ''

  page_symb
.endfields
