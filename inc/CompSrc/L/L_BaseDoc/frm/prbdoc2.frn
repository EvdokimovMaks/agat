  olHistory.sGetField(coKatBank, loBD.BankPol.NRec, 'NAME', BaseDoc.dDoc)
  olHistory.sGetField(coKatBank, loBD.BankPol.NRec, 'ADDR', BaseDoc.dDoc)
  GetKatOrgAttrName4Rep('UNN', 0, GetStateRef(ccKaz))
  BankPolRNN
