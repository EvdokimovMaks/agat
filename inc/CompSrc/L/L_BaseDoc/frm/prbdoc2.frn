    //оплата товара
    wTypeAttr   := GetClassCode(coBaseDoc, 'Оплата товара');

    if (wTypeAttr = 0)
      OplTov := ''
    else
      OplTov := GetSegName(wTypeAttr, BaseDocNRec);
  }
