  oHistory.sGetField(coKatOrg, loBD.Pol.nRec, 'NAME', loBD.BaseDoc.dDoc)
  GetKatOrgAttrName4Rep('OKPO', loBD.Pol.nRec)
  loBD.Pol.OKPO
  loBD.Pol.Addr+', '+loBD.CityPol.Name
  olHistory.sGetField(coKatBank, loBD.BankPol.NRec, 'SCHET1', BaseDoc.dDoc)
  GetKatOrgAttrName4Rep('UNN', loBD.Pol.nRec)
  loBD.Pol.UNN
  NalOrgAddrImp

!  Реквизиты третьего лица:
!  Наименование -                    Б^ Б
!  Код ОКПО -                        Б^ Б
!  Адрес -                           Б^ Б
!  Номер банковского счета -         Б^ Б
!  РНН -                             Б^ Б
!  Адрес налогового органа -         Б^ Б
  if(not IsPosr, '', oHistory.sGetField(coKatOrg, loBD.Posr.nRec, 'NAME', loBD.BaseDoc.dDoc))
  if(not IsPosr, GetKatOrgAttrName4Rep('OKPO', 0, GetStateRef(ccKaz)), GetKatOrgAttrName4Rep('OKPO', loBD.Posr.nRec))
  if(not IsPosr, '', loBD.Posr.OKPO)
  if(not IsPosr, '', loBD.Posr.Addr+', '+loBD.CityPosr.Name)
  if(not IsPosr, '', olHistory.sGetField(coKatBank, loBD.BankPosr.NRec, 'SCHET1', BaseDoc.dDoc))
  if(not IsPosr, GetKatOrgAttrName4Rep('UNN', 0, GetStateRef(ccKaz)), GetKatOrgAttrName4Rep('UNN', loBD.Posr.nRec))
  if(not IsPosr, '', loBD.Posr.UNN)
  NalOrgAddr3

!  Реквизиты иностранного покупателя:
!  Наименование -                    Б^ Б
!  Страна -                          Б^ Б
!  Адрес -                           Б^ Б
!  Реквизиты банка покупателя -      Б^ Б
  oHistory.sGetField(coKatOrg, loBD.Post.nRec, 'NAME', loBD.BaseDoc.dDoc)
  loBD.StatePost.Name
  loBD.Post.Addr+', '+loBD.CityPost.Name
  olHistory.sGetField(coKatBank, loBD.BankPlat.NRec, 'NAME', BaseDoc.dDoc)+
    if(olHistory.sGetField(coKatBank, loBD.BankPlat.NRec, 'ADDR'  , BaseDoc.dDoc) = '', '', ', '+
       olHistory.sGetField(coKatBank, loBD.BankPlat.NRec, 'ADDR'  , BaseDoc.dDoc))+
    if(olHistory.sGetField(coKatBank, loBD.BankPlat.NRec, 'SCHET1', BaseDoc.dDoc) = '', '', ', р/с'+
       olHistory.sGetField(coKatBank, loBD.BankPlat.NRec, 'SCHET1', BaseDoc.dDoc))

!  Реквизиты и условия контракта:
!  Номер -                    Б@@@@@@@@@@@@@@@@@@ Б   Последняя дата -       Б^ Б
!  Дата -                     Б@@@@@@@@@@@@@@@@@@ Б   Валюта платежа -       Б^ Б
!  Сумма контракта -          Б&'&&&&&&&&&&&&&.&& Б   Код валюты платежа -   Б^ Б
!  Товар -                    Б@@@@@@@@@@@@@@@@@@ Б   Форма расчетов -       Б^ Б
!  Валюта по контракту -      Б@@@@@@@@@@@@@@@@@@ Б
!  Код валюты по контракту -  Б^ Б
!  Валютная оговорка -        Б^ Б
  loBD.BaseDoc.NoDoc DateToStr(Add_Day(loBD.BaseDoc.dDoc, loBD.BaseDoc.KolDn), 'DD/MM/YYYY')
  loBD.BaseDoc.dDoc  if(loBD.BaseDoc.cVal = 0, sGetTune('NDE.SimvRub'), loBD.KlVal.Dollar)
  loBD.BaseDoc.Summa if(loBD.BaseDoc.cVal = 0, sGetTune('NDE.RublISO'), loBD.KlVal.ISO)
  Tovar loBD.KatPayment.Name
  if(loBD.SpStep.cVal = 0, sGetTune('NDE.SimvRub'), loBD.SpVal.Dollar)
  if(loBD.SpStep.cVal = 0, sGetTune('NDE.RublISO'), loBD.SpVal.ISO)
  ValOgovorka

  ''  Catalogs.Name     ''
  ''  sGetTune('Boss')  ''
  BankNotes

  page_symb
.endfields
