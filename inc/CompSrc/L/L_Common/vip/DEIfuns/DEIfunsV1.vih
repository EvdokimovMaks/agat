                _wTipKolSrc      : word;    // тип количества источника
                _KolSrc          : double;  // количество источника в отпускных
                _wTipKolDest     : word;    // тип количества приемника
                _KolDest         : double   // количество приемника в отпускных
          );

//******************************************************************************
#doc
