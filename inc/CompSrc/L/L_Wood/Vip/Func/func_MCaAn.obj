Table STRUCT FrPar ""
(
  nRec       : comp           ""
, cRec       : comp           "Ссылка на настройку"
, wTip       : word           "Тип выгрузки" //0 - XLS, 1 - XML
, sField     : string[80]     "Поле"
, lPole      : longint        "значение"
, dPole      : double         "значение"
, sPole      : string         "значение"
, cPole      : comp           "значение"
)
With Index
(
  FrPar01  =  Nrec(Unique, Surrogate)
, FrPar02  =  cRec + wTip + sField
);
