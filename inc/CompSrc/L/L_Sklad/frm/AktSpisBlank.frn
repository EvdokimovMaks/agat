#doc
Акт о списании бланков строгой отчетности (для бюджетных организаций)
#end
.linkform 'AktSpisBlank' prototype is printaktspis
.DEFP PA4
.DEFO PORTRAIT
.PAPER PA4
.ORIENT PORTRAIT
.LL 71
.ORIENT LANDSCAPE
.LL 51
.nameinlist 'Акт о списании бланков строгой отчетности (для бюджетных организаций)'
.Group 'AktSpis'
#include mfo.frn
#CommissionLinkVar
.var
 sKomis1, sKomis2, sKomis3, sKomis4 : string;
.endvar
.{
#CommissionLinkInit(printaktspis)
.begin
  sKomis1 := Председатель_комиссии_ФИО + if(Председатель_комиссии_Должность <> '', '(' + Председатель_комиссии_Должность + ')', '') +
    if(Массив_Член_комиссии_ФИО[1] <> '', ', ' + Массив_Член_комиссии_ФИО[1] + '(' + Массив_Член_комиссии_Должность[1] + ')' , '') +
    if(Массив_Член_комиссии_ФИО[2] <> '', ', ', '');
  sKomis2 :=
    if(Массив_Член_комиссии_ФИО[2] <> '', Массив_Член_комиссии_ФИО[2] + '(' + Массив_Член_комиссии_Должность[2] + ')' , '') +
    if(Массив_Член_комиссии_ФИО[3] <> '', ', ' + Массив_Член_комиссии_ФИО[3] + '(' + Массив_Член_комиссии_Должность[3] + ')' , '') +
    if(Массив_Член_комиссии_ФИО[4] <> '', ', ' + Массив_Член_комиссии_ФИО[4] + '(' + Массив_Член_комиссии_Должность[4] + ')' , '') +
    if(Массив_Член_комиссии_ФИО[5] <> '', ', ', '');
  sKomis3 :=
    if(Массив_Член_комиссии_ФИО[5] <> '', Массив_Член_комиссии_ФИО[5] + '(' + Массив_Член_комиссии_Должность[5] + ')' , '');
end.
.fields
  Nom
  DateToStr(Дата, 'DD mon YYYY')
  DateToStr(Дата, 'DD/MM/YYYY')

  Организация
    + ' Адрес:' + мой_Адрес
    + ' Банк:'  + мой_БАНК_Имя
    + ' р/с '   + мой_БАНК_Счет
    + mfo_      + мой_БАНК_Мфо

  ОКПО
  if(МОЛ_Откуда_Д<>'', МОЛ_Откуда_Д, '')
  DebSch
  KredSch
  sKomis1 sKomis2 sKomis3 sKomis4
!.{
  Код
  Наименование
  причина_списания
!.}
.endfields

