#include FrmDopFunc.frn
.function if01(d_: double): double;
begin
  if01 := if(abs(d_) < GetPresision_forKol,
             if(Признак_описи, d_, 1),
             d_);
end.
.function if02(d_: double): double;
begin
  if02 := if(abs(d_) < GetPresision_forKol, 1, d_);
end.
