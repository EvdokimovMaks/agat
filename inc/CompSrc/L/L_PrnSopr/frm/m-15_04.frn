!подсчет количества которое надлежит отпустить
.Create view SpecM15
from
  KatSoprMemTTN
Where
((
  comp(накладная_нрек) == KatSoprMemTTN.nRec
))
;
.begin
  if (SpecM15.GetFirst KatSoprMemTTN = tsOk)
    isReklNakl := (SpecM15.KatSoprMemTTN.VidSopr = 106) OR (SpecM15.KatSoprMemTTN.VidSopr = 206)
  else
    isReklNakl := FALSE;
end.
