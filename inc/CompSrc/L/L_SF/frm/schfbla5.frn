  if (longint(dFact)<>0, DateToStr(dFact, '"DD" Mon YYYY г.'), '')

  if (Direct = 1, MyOrgName, OrgName)
  If (Direct = 1, MyOrgAddr, OrgAddr)
  UnnOrInn
  If (Direct = 1 ,MyOrgINN, OrgINN)
  strSoprDoc

  if (Direct <> 1, MyOrgName, OrgName)
  if (Direct <> 1, MyOrgAddr, OrgAddr)
  UnnOrInn
  if (Direct <> 1, MyOrgINN, OrgINN)
  if (isNDEEqual, sValNDE, SimvolV) if (isNDEEqual, sValNDE, SimvolV)
  if (isNDEEqual, sValNDE, SimvolV) if (isNDEEqual, sValNDE, SimvolV)
.endfields
