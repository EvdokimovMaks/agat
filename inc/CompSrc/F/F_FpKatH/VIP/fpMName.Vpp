#ifndef _FPMNAME_VPP_
#define _FPMNAME_VPP_

//------------------------------------------------------------------------------
Function GetFPMarkerName(coTbl : LongInt) : string;
{ GetFPMarkerName := 'GetSomeFpMarker';
  case coTbl of
      cofpBudget : GetFPMarkerName := 'GetSomeFpBudget';
        coFilial : GetFPMarkerName := 'GetSomeFilial';
          cofpCO : GetFPMarkerName := 'GetSomeFpCO';
      cofpGrTemp : GetFPMarkerName := 'GetSomeFpGrTemp';
       cofpStBud : GetFPMarkerName := 'GetSomeFpStBud';
      cofpPeriod : GetFPMarkerName := 'GetSomeFpPeriod';
        cofpTPer : GetFPMarkerName := 'GetSomeFPTPer';
      cofpVarian : GetFPMarkerName := 'GetSomeFPVar';
        cofpTPod : GetFPMarkerName := 'GetSomeFPTPod';
        coFpRasp : GetFPMarkerName := 'GetSomeFPRasp';
      coAllIndex : GetFPMarkerName := 'GetSomeIndex';
       cofpTForm : GetFPMarkerName := 'GetSomeFpTForm';
      cofpSpAxTF : GetFPMarkerName := 'GetSomeFpSpAx';
       cofpMRasp : GetFPMarkerName := 'GetSomeFpMRasp';
      cofpKatImp : GetFPMarkerName := 'GetSomeFpKatImp';
      coFpMeanTF : GetFPMarkerName := 'GetSomeFpMeanTF';
      coFpBudVar : GetFPMarkerName := 'GetSomeFpBudVar';
      coKatExpr  : GetFPMarkerName := 'GetSomeKatExpr';
      coFpArea   : GetFPMarkerName := 'GetSomeFpArea';
      coFpOborot : GetFPMarkerName := 'GetSomeFpOborot';
          coSloj : GetFPMarkerName := 'GetSomeFpSloj';
        coSpSloj : GetFPMarkerName := 'GetSomeFpSpSloj';
  coFpBudProcces : GetFPMarkerName := 'GetSomeFpBudProcces';
      coKatVarPl : GetFPMarkerName := 'GetSomeKatVarPl';
       coKatSmet : GetFPMarkerName := 'GetSomeKatSmet';
  end;
}

#endif//_FPMNAME_VPP_
