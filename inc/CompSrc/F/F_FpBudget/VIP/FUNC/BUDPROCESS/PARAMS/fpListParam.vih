#ifndef _ObjFpListParam_Included_
#define _ObjFpListParam_Included_

#ifdef  ComponentVersion
#component "F_FpBudget"
#endif//ComponentVersion

//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
#doc
Параметры списков бюджетов<br>
<b>Внимание!</b> Интерфейс предназначен только для внутреннего использования<br>
#end
#IntPrivate ObjInterface iFpListParam;

//------------------------------------------------------------------------------
#doc
   Получить в каком режиме задаются данные (0 - оба режима; 1 - один реквизит копии; 2 - список реквизитов копии)<br>
#end
//------------------------------------------------------------------------------
Function GetDataModeListParam : word;

//------------------------------------------------------------------------------
#doc
   Получить режим списка бюджетов
#end
Function GetMode1 : word;

//------------------------------------------------------------------------------
#doc
   Выставить всем параметрам ссылку на родителя _wParent = 0 (Роль А) = 1 (Роль Б)
#end
Function SetParentDefaults(_wParent : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Провести смещение периодов в списке бюджетов<br>
   <ul>
   <li> _cTPer  - тип периода
   <li> _iShift - смещение
   </ul>
#end
Function DoShiftPer(_cTPer : comp; _iShift : integer) : boolean;

//------------------------------------------------------------------------------
#doc
   Загрузить настройки интерфейса<br>
#end
Function Read_ITune(_cTune : comp; _wEntity : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Сохранить настройки интерфейса<br>
#end
Function Save_ITune(_cTune : comp; _wEntity : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Удалить настройки интерфейса<br>
#end
Function Del_ITune(_cTune : comp; _wEntity : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Прочитать из хранилища только список бюджетов
#end
Function ReadOnlyList_ITune(_cTune : comp; _wEntity : word) : boolean;

//------------------------------------------------------------------------------
#doc
   Инициализация параметров интерфейса<br>
#end
Function InitParam
(
   _wModeN : word   // Вид списка бюджетов:  0 - только источник; 1 - источник и приемник
): boolean;

//------------------------------------------------------------------------------
#doc
   Проверка видимости полей<br>
#end
Function CheckFieldState : boolean;

//------------------------------------------------------------------------------
#doc
   Функция для обновления в списке бюджетов ЦО
#end
Function UpdateUserListByCO(_cPer : comp; _Variant : comp; _sStage : string) : boolean;

//------------------------------------------------------------------------------
#doc
   Функция для обновления в списке бюджетов периода
#end
Function UpdateUserListByPer(_cCO : comp; _Variant : comp; _sStage : string) : boolean;

//------------------------------------------------------------------------------
#doc
   Функция для обновления в списке бюджетов варианта
#end
Function UpdateUserListByVar(_cCO : comp; _cPer    : comp; _sStage : string) : boolean;

//------------------------------------------------------------------------------
#doc
   Функция для обновления в списке бюджетов стадии
#end
Function UpdateUserListByStage(_cCO : comp; _cPer  : comp; _Variant : comp) : boolean;

//------------------------------------------------------------------------------
#doc
   Функция для обновления списка бюджетов в соответствии с заданными параметрами
#end
Function UpdateUserListByParam(_cCO : comp; _cPer  : comp; _Variant : comp; _sStage : string; _cSlCO : comp; _cSlPer  : comp; _SlVariant : comp; _sSlStage : string) : boolean;


//------------------------------------------------------------------------------
#doc
   Функция перерисовки списка бюджетов
#end
Function RescanAllList : boolean;

//------------------------------------------------------------------------------
#doc
   Функция удаления списка бюджетов
#end
Function DeleteAllList : boolean;

//------------------------------------------------------------------------------
#doc
   Вставка в список бюджетов записи по заданным параметрам (используется для построения автодекарта)<br>
#end
Function InsertInList(_cCO : comp; _cPer  : comp; _Variant : comp; _sStage : string; _wKodReg : word;
                      _cSlCO : comp; _cSlPer  : comp; _SlVariant : comp; _sSlStage : string; _wSlKodReg : word; _wReplace : word
                     ) : boolean;

//------------------------------------------------------------------------------
#doc
   Функция обновления списка бюджетов в соответствии с настройками при загрузке данных
#end
Function UpdateListOnLoad : boolean;

//------------------------------------------------------------------------------
// EVENTS
//------------------------------------------------------------------------------
#doc
   Событие при обновлении заданного списка бюджетов<br>
#end
Event Function Event_ChangeUserList(_wUserList : word; _isSlaveMode : boolean) : boolean;

//------------------------------------------------------------------------------
#doc
   Событие при обновлении автодекарта списка бюджетов<br>
#end
Event Function Event_ChangeAutoList(_wAutoList : word; _isSlaveMode : boolean) : boolean;

//------------------------------------------------------------------------------
#doc
   Событие при сохранении списка бюджетов<br>
#end
Event Function Event_SaveList : boolean;

//------------------------------------------------------------------------------
#doc
   Событие при перегрузке списка бюджетов из хранилища данных<br>
#end
Event Function Event_ReReadList : boolean;

//------------------------------------------------------------------------------
#doc
   Событие при открытии сценария модели<br>
#end
Event Function Event_OpenScenario : boolean;

//------------------------------------------------------------------------------
#doc
   Событие при установке ссылки на родительские значения<br>
#end
Event Function Event_SetParentDefaults : boolean;

