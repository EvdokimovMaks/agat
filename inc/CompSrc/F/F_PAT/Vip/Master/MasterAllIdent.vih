#ifndef __MasterAllIdent__vih__
#define __MasterAllIdent__vih__

#ifdef ComponentVersion
#Component "F_Pat"
#end

type TRecParam = record
    // идентификатор TXO и ключи ТXО
    myTiDk            : word;
    mySysOper         : word;
    myHozOper         : comp;
    myPickModeHozForm : word;
    // идентификатор для ПАТ-ов
    myPickModePAT     : word;
    // идентификатор типовых алгоритмов и констант
    myPickModeTypical : word;
end;

#doc
Объектный интерфейс для общего мастера ключей
#end
ObjInterface ObjIdent;
  #doc
  Событие приходит при изменении параметров ключа
  #end
  Event procedure ObjIdent_WasIdentChanged;

  #doc
  Событие приходит при нажатии клавиши "ввод"
  #end
  Event procedure ObjIdent_Ok(sIdent: string);

  #doc
  входные параметры
  #end
  function SetParams(_Param : tRecParam) : boolean;

  #doc
  выходные параметры
  #end
  function GetParams : string;
end;

#end // __MasterAllIdent__vih__
