// Структура соответствия
Type rcAnRelStruct = record
  wKau  : array[1..4] of word;// коды аналитик источников // 1- счет/субсчет
  wKauD : word;               // приемник
  cPlSch: Comp;               // план счетов
  sName : string;             // имя соответствия
end;

//------------------------------------------------------------------------------
// Структура для поиска по соответствию
Type rcSearchAnRel = record
  cKau  : array[1..3] of comp; // значения аналитик источников // 1- счет/субсчет
  Schet : tSchet3;             // счет
  SubSch: tSubSch;             // субсчет
  cPlSch: Comp;                // план счетов
  sName : string;              // имя соответствия
end;
