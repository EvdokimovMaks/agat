#component "F_TXO"
alter interface getHozOper;
window wihozoper;
  handleevent
    cmAlt1:
    {
      if curtable = #fpobor
      {
        if fpobor.nrec = 0
        {
          message('Заполните шаблон проводки');
          exit;
        }
        runinterface(F_TXO::TXODSQL, fpobor.nrec, 9074, false, 0);
      }
    }
  end;
end;
end.
