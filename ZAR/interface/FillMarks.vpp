function FillMarks: boolean;
{
  var Result: boolean;
  Result := PAF.CheckPersonalAccountFilter(0);
  FillMarks := Result;
  if (not Result)
  {
    SelectField(#FiltrPublic);
    Message('Не задан фильтр по работникам', Error + OkButton);
  }
  ReReadRecord(#ZAR_USER);
}

