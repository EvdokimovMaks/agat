#include createtune.vih

#component "AGAT"

Handler with replace AGATZAR_CreateTuneComponent
on extensionPoint epCreateTuneComponent (TR : ObjCreateTuneEx)
Action
{
  Result := true;
  TR.AddPart(0, 'AGAT.ZAR', '', 'Агат.Управление персоналом и заработная плата', 0);
  TR.AddTune(ttUSERTUNE, 'AGAT.ZAR.PLANVACREPENABLED'  , 'AGAT.ZAR', 'Разрешить отчет "Реестр сотрудников с запланированными отпусками за период"', ftList, '0', '', 0);
    TR.AddList(0, 'Нет');
    TR.AddList(1, 'Да');
}
