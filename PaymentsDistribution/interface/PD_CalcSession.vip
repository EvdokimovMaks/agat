#include PD_CalcSession.vih
#include PD_Row.vih

#component "PD_AGAT"

interface PD_CalcSession;

type TByteArray = array[0..0] of byte;

var
  _cells: array[0..0] of TByteArray;

procedure add(cell: PD_ICell);
{
  _cells[cell.row.id][cell.id] := 1;
}

function isCalculated(cell: PD_ICell): boolean;
{
  result := _cells[cell.row.id][cell.id] = 1;
}

end.
