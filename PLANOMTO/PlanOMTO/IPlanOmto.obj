#ifndef __IPlanOmto_Obj__
#define __IPlanOmto_Obj__

#component "AGAT"

table struct local tPlanOmto (
  ClaimNrec         : comp,
  IndentNrec        : comp,
  ClaimState        : word,
  ClaimStateNrec    : comp,
  KatpodrNrec       : comp,
  DeliverOrgNrec    : comp,
  KatstroyNrec      : comp,
  CostitemNrec      : comp,
  PersonNrec        : comp,
  DeviceNrec        : comp,
  GroupmcNrec       : comp,
  KatmcNrec         : comp,
  OtpedNrec         : comp,
  BasedocNrec       : comp,
  Tu                : string,
  Description       : string,
  DeliverClaimNumber: string[40],
  ClaimQty          : double,
  ClaimPrice        : double,
  SaldoQty          : double,
  RegradeQty        : double,
  BasedocQty        : double,
  BasedocSum        : double,
  PaidSum           : double,
  DeliveredQty      : double,
  IssuedQty         : double,
  IndentDate        : date,
  IndentDeliverDate : date,
  ClaimAcceptDate   : date,
  DeliverDatePlan   : date,
  DeliverDateChanged: date,
  DeliverDateFact   : date,
  BasedocDate       : date,
