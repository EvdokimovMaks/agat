#include orgkpp.vih;

#component "AGAT";

interface OrgKPPView;

var
  _model: IOrgKPP;

create view
var
  _inn: string;
as select *
from _model
bounds byINN = _inn == _model.inn;

browse brOrgKPP;
table _model;
fields
  _model.INN 'ИНН': [20], protect;
  _model.KPP 'КПП': [20], noprotect;
  _model.Description 'Примечание': [50], noprotect;
end;

property Model: IOrgKPP absolute _model;

end.
