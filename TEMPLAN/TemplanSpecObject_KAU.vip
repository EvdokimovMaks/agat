function oTemplanSpecObject.GetKey: string;
{
  result := string(STBUD_SIRIE_I_MATERIALI,0,0) + ', ' + string(STBUD_POKUPNIE_POLUFABRIKATI,0,0) + ', ' +  string(STBUD_POLUFABRIKARI_SOBSTVENNOGO_PROIZVODSTVA,0,0) + ', ' +
