#include system.vih

#component "system"

interface NotifyCollectionChangedEventArgs;

var
  _action: byte;
  _items: IList;

constructor init;
{
  result := false;
  if not loadvipref(_items,'system::list') exit;
  result := true;
}

property Action: byte absolute _action;
property Items: IList read _items;

end.
