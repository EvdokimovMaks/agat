#include system.vih

#component "system"

interface NotifyCollectionChangedEventArgs;

var
  _action: byte;
  _oldItems: IList;
  _newItems: IList;

constructor init;
{
  result := false;
  if not loadvipref(_oldItems,'system::list') exit;
  if not loadvipref(_newItems,'system::list') exit;
  result := true;
}

property Action: byte absolute _action;
property OldItems: IList read _oldItems;
property NewItems: IList read _newItems;

end.
