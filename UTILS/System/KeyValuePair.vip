#include System.vih

#component "System"

interface KeyValuePair;

var
  _key: variant;
  _value: IObject;

constructor KeyValuePair(aKey: variant; aValue: IObject);
{
  _key := aKey;
  _value := aValue;
  result := true;
}

property Key: variant absolute _key;
property Value: IObject absolute _value;

end.
