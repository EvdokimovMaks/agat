#include utest.vih
#include LogService.vih

#component "TEST"

vipinterface TEST_LogService implements oUTest, oLogServiceSet;
interface TEST_LogService;
  var
    _logService: oLogService;

  procedure SetLogService(logService: oLogService); { _logService := logService; }

  function GetDescription: string; { result := 'utils/tests/test_LogService.vip TEST_oLogService'; }

  function Test: boolean;
  {
    if (_logService != nullref)
    {
      _logService.Log('test log message');
      result := true;
    }
  }
end.
